    952            /*The ADC transfer function is a linear equation defined as follows:
    953              V(cell) = GAIN x ADC(cell) + OFFSET
    954              If the DEVICE_XREADY is set, the voltage register values from the device should not be used. If
    955            */ 
    956          void Afe_Volt_Val_Get(void)
    957          { 
    958            uint8_t i = 0;
    959            uint16_t volt_sum = 0;
    960            uint16_t volt_max = 0;
    961            uint16_t volt_min = 0xFFFF;
    962            //====================================250mS
    963            if(Cell_Volt_Sample_Cnt >= 26)
    964            {
    965              Cell_Volt_Sample_Cnt = 0;
    966              for(i =0;i< 20;i += 2)
    967              {  
    968                Adc_value[i/2] = Afe_Get_Adc((VC1_HI_ADDR + i)) & 0x3FFF; //VC1_HI_ADDR =0x0C
    969                Cell_Volt[i/2] = (uint16_t)((uint32_t)ADCGain_Val *Adc_value[i/2]/1000) + ADCOffset_Val;
    970                volt_sum += Cell_Volt[i/2];
    971                if(Cell_Volt[i/2] < volt_min)
    972                {
    973                   volt_min = Cell_Volt[i/2];
    974                }
    975                if(Cell_Volt[i/2] > volt_max)
    976                {
    977                   volt_max = Cell_Volt[i/2];
    978                }
    979              } 
    980              Cell_Volt_Tol = volt_sum; 
    981              Cell_Volt_Avg = Cell_Volt_Tol/10; 
    982              Cell_Volt_Max = volt_max; 
    983              Cell_Volt_Min = volt_min; 
    984                
    985            }
    986            //====================================250mS
    987            /*  Once converted to digital form, each cell voltage is added up and the summation result stored in the BAT
    988                registers. This 16-bit value has a nominal LSB of 1.532 mV. */
    989            //Pack_Volt = (uint16_t)((uint32_t)4 *ADCGain_Val *Afe_Get_Adc(BAT_HI_ADDR)/1000+(10 *ADCOffset_Val));
    990            //Pack_Volt = (uint16_t)((uint32_t)1532 *Afe_Get_Adc(BAT_HI_ADDR)/1000);
    991            
    992            //====================================250mS
    993            //Positive values are charge current; negative values are discharge current.
    994            //an average over the 250-ms integration period.
    995            /*Why does the CC value read not change when I set the CC_ONESHOT bit?
    996                  The CC_READY bit must be cleared before the CC_ONESHOT bit is set. Note also that the CC_EN bit
    997                  should be cleared or a CC conversion may already be in progress.
    998              The full scale range of the CC is ± 270 mV, with a max recommended input range of ± 200 mV, thus yielding an LSB of approximately 8.44 μV.
    999              The following equation shows how to convert the 16-bit CC reading into an analog voltage if no boardlevel calibration is performed:
   1000              CC Reading (in μV) = [16-bit 2’s Complement Value] × (8.44 μV/LSB) 
   1001            */
   1002            
   1003            I2C_Read(SYS_STAT_ADDR,&SYS_STAT_Last); 
   1004            SYS_STAT.Byte = SYS_STAT_Last; 
   1005            if((CC_Volt_Sample_Cnt >= 26) && SYS_STAT.Bit.CC_READY) //(0)//
   1006            {
   1007              CC_Volt_Sample_Cnt = 0;
   1008              SYS_STAT.Bit.CC_READY = 0;
   1009              CC_AD = Afe_Get_Adc(CC_HI_ADDR);  
   1010              //CC_Val = (int32_t)820 * CC_AD /100; //mA (int32_t)
   1011              CC_Val = (int32_t)820 * CC_AD /500; //mA (int32_t)
   1012               
   1013              Afe_CC_1Shot_Set();
   1014              if(CC_Val < -320000)
   1015              {
   1016                CC_Val = -320000;
   1017              }
   1018              else if(CC_Val > 320000)
   1019              {
   1020                CC_Val = 320000;
   1021              }
   1022              Current_Val = CC_Val;
   1023              if(CC_Val < 0)
   1024              {
   1025                Current_Val = -CC_Val;
   1026              }
   1027            }
   1028            //====================================2S 
   1029            if(Temp_Volt_Sample_Cnt >= 210)
   1030            {
   1031              //V_TS2_Val = (uint16_t)((uint32_t)(382 * ((uint16_t)adcval <<8 | tmpval))/1000);//mV
   1032              V_TS2_Val = Afe_Get_Adc(TS2_HI_ADDR) &0x3FFF ;
   1033              V_TS2_Val = (uint16_t)(((uint32_t)382 * V_TS2_Val /1000));  //mV
   1034              V_TS2_Val = (uint16_t)(((uint32_t)4096 * V_TS2_Val /3300));  //mV
   1035              //R_TS2_Val = (uint16_t)((uint32_t)10000 * V_TS2_Val)/(3300 - V_TS2_Val); 
   1036            }
   1037            //=====================================================================================
   1038            
   1039          }
