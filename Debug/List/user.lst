    422          /*
    423            The following equations show how to use the 14-bit ADC readings in TS1, TS2, and TS3 to determine the
    424            resistance of the external 103AT thermistor:
    425              VTSX = (ADC in Decimal) x 382 μV/LSB                     (4)
    426              RTS = (10,000 × VTSX) ÷ (3.3 C VTSX)                   (5)
    427          */
    428          void TempCheck(void)
    429          { 
    430              static uint8_t ChgTemp_cnt =0;
    431              static uint8_t DisTemp_cnt =0;  
    432               
    433              Temp_Val = V_TS2_Val;//R_TS2_Val;
    434              if(WorkMode == CHARGE_MODE)//if(Bits_flag.Bit.Chg)
    435              {
    436                 if((Temp_Val >ChgTempL_ON) || (Temp_Val < ChgTempH_ON) || (ChgTemp_cnt >= 10))
    437                 {
    438                   if((ChgTemp_cnt ++) >= 10)
    439                   {
    440                     ChgTemp_cnt = 10;
    441                     Bits_flag.Bit.ChgTemp = 1;
    442                   }
    443                 }
    444                 else
    445                 {
    446                   ChgTemp_cnt = 0;
    447                 }
    448                //==========================充电温度保护恢复
    449                if(Bits_flag.Bit.ChgTemp && (Temp_Val < ChgTempL_OFF) && (Temp_Val >ChgTempH_OFF))
    450                {
    451                    ChgTemp_cnt = 0;
    452                    Bits_flag.Bit.ChgTemp = 0;
    453                } 
    454              }
    455              else if(WorkMode == DISCHARGE_MODE)//(Bits_flag.Bit.Dis)
    456              {
    457                ChgTemp_cnt = 0;
    458                if((Temp_Val > DisTempL_ON) || (Temp_Val < DisTempH_ON) || (DisTemp_cnt >= 10)) 
    459                {
    460                  if((DisTemp_cnt ++) >= 10)
    461                  {
    462                    DisTemp_cnt = 10;
    463                    Bits_flag.Bit.DisTemp = 1;
    464                  }
    465                }
    466                else
    467                {
    468                  DisTemp_cnt = 0;
    469                }  
    470                //==========================放电温度保护恢复
    471                if(Bits_flag.Bit.DisTemp && (Temp_Val < DisTempL_OFF) && (Temp_Val > DisTempH_OFF))
    472                {
    473                  DisTemp_cnt = 0;
    474                  Bits_flag.Bit.DisTemp = 0;
    475                } 
    476              }   
    477          }
