    965            /*The ADC transfer function is a linear equation defined as follows:
    966              V(cell) = GAIN x ADC(cell) + OFFSET
    967              If the DEVICE_XREADY is set, the voltage register values from the device should not be used. If
    968            */ 
    969          void Afe_Volt_Val_Get(void)
    970          { 
    971            uint8_t i = 0;
    972            uint16_t volt_sum = 0;
    973            uint16_t volt_max = 0;
    974            uint16_t volt_min = 0xFFFF;
    975            //====================================250mS
    976            if(Cell_Volt_Sample_Cnt >= 26)
    977            {
    978              Cell_Volt_Sample_Cnt = 0;
    979              for(i =0;i< 20;i += 2)
    980              {  
    981                Adc_value[i/2] = Afe_Get_Adc((VC1_HI_ADDR + i)) & 0x3FFF; //VC1_HI_ADDR =0x0C
    982                Cell_Volt[i/2] = (uint16_t)((uint32_t)ADCGain_Val *Adc_value[i/2]/1000) + ADCOffset_Val;
    983                volt_sum += Cell_Volt[i/2];
    984                if(Cell_Volt[i/2] < volt_min)
    985                {
    986                   volt_min = Cell_Volt[i/2];
    987                }
    988                if(Cell_Volt[i/2] > volt_max)
    989                {
    990                   volt_max = Cell_Volt[i/2];
    991                }
    992              } 
    993              Cell_Volt_Tol = volt_sum; 
    994              Cell_Volt_Avg = Cell_Volt_Tol/10; 
    995              Cell_Volt_Max = volt_max; 
    996              Cell_Volt_Min = volt_min; 
    997                
    998            }
    999            //====================================250mS
   1000            /*  Once converted to digital form, each cell voltage is added up and the summation result stored in the BAT
   1001                registers. This 16-bit value has a nominal LSB of 1.532 mV. */
   1002            //Pack_Volt = (uint16_t)((uint32_t)4 *ADCGain_Val *Afe_Get_Adc(BAT_HI_ADDR)/1000+(10 *ADCOffset_Val));
   1003            //Pack_Volt = (uint16_t)((uint32_t)1532 *Afe_Get_Adc(BAT_HI_ADDR)/1000);
   1004            
   1005            //====================================250mS
   1006            //Positive values are charge current; negative values are discharge current.
   1007            //an average over the 250-ms integration period.
   1008            /*Why does the CC value read not change when I set the CC_ONESHOT bit?
   1009                  The CC_READY bit must be cleared before the CC_ONESHOT bit is set. Note also that the CC_EN bit
   1010                  should be cleared or a CC conversion may already be in progress.
   1011              The full scale range of the CC is ± 270 mV, with a max recommended input range of ± 200 mV, thus yielding an LSB of approximately 8.44 μV.
   1012              The following equation shows how to convert the 16-bit CC reading into an analog voltage if no boardlevel calibration is performed:
   1013              CC Reading (in μV) = [16-bit 2’s Complement Value] × (8.44 μV/LSB) 
   1014            */
   1015            if((CC_Volt_Sample_Cnt >= 26) && SYS_STAT.Bit.CC_READY) //(0)//
   1016            {
   1017              CC_Volt_Sample_Cnt = 0;
   1018              SYS_STAT.Bit.CC_READY = 0;
   1019              CC_AD = Afe_Get_Adc(CC_HI_ADDR);  
   1020              //CC_Val = (int32_t)820 * CC_AD /100; //mA (int32_t)
   1021              CC_Val = (int32_t)820 * CC_AD /500; //mA (int32_t)
   1022               
   1023              Afe_CC_1Shot_Set();
   1024              if(CC_Val < -320000)
   1025              {
   1026                CC_Val = -320000;
   1027              }
   1028              else if(CC_Val > 320000)
   1029              {
   1030                CC_Val = 320000;
   1031              }
   1032              Current_Val = CC_Val;
   1033              if(CC_Val < 0)
   1034              {
   1035                Current_Val = -CC_Val;
   1036              }
   1037            }
   1038            //====================================2S 
   1039            if(Temp_Volt_Sample_Cnt >= 210)
   1040            {
   1041              //V_TS2_Val = (uint16_t)((uint32_t)(382 * ((uint16_t)adcval <<8 | tmpval))/1000);//mV
   1042              V_TS2_Val = Afe_Get_Adc(TS2_HI_ADDR) &0x3FFF ;
   1043              V_TS2_Val = (uint16_t)(((uint32_t)382 * V_TS2_Val /1000));  //mV
   1044              V_TS2_Val = (uint16_t)(((uint32_t)4096 * V_TS2_Val /3300));  //mV
   1045              //R_TS2_Val = (uint16_t)((uint32_t)10000 * V_TS2_Val)/(3300 - V_TS2_Val); 
   1046            }
   1047            //=====================================================================================
   1048            
   1049          }
