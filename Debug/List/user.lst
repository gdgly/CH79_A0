    730          void ModeCheck(void)
    731          {       
    732            static uint8_t ModeChange_Lock = 0;
    733            //工作模式的检测是依据外部Triger端的电压大小来判定
    734            
    735            // 问题3：（充放电及空载识别问题）欠压状态下，插上充电器时，识别不了充电器;  
    736                    //解决方案：检测无Triger信号时，为空载模式;
    737                                //Triger信号范围在[220,630]时【[350,580]】，或检测到有负载信号、或放电电流 >= 10mA 时，为放电模式，否则为充电模式.
    738            
    739          //==========================================================================
    740          //== OV_val为过充保护电压值（mV）、 UV_val为过放保护电压值（mV）
    741          //void Afe_OV_UV_Threshold_Set(uint16_t OV_val, uint16_t UV_val)
    742            
    743            Check_Val = ADConverse(3); 
    744            if(Check_Val < 10)// 无信号
    745            { 
    746              if(IdleExchangeMode_Cnt >= 100)
    747              { 
    748                IdleExchangeMode_Cnt = 100;
    749                ChgExchangeMode_Cnt = 0;
    750                DisExchangeMode_Cnt = 0;
    751                WorkMode = IDLE_MODE;
    752                if(ModeChange_Lock != 0)
    753                {
    754                  ModeChange_Lock = 0;
    755                  Afe_OV_UV_Threshold_Set(OV_THREHOLD_VAL_SET, UV_THREHOLD_VAL_SET); 
    756                }
    757              }
    758            } 
    759            else if((Check_Val >= 220 && Check_Val < 630) || SYS_CTRL1.Bit.LOAD_PRESENT || CC_Val < (-10) )
    760            {  
    761              if(DisExchangeMode_Cnt >= 30)
    762              { 
    763                IdleExchangeMode_Cnt = 0;
    764                ChgExchangeMode_Cnt = 0;
    765                DisExchangeMode_Cnt = 100; 
    766                WorkMode = DISCHARGE_MODE;
    767                if(ModeChange_Lock != 2)
    768                {
    769                  ModeChange_Lock = 2;
    770                  // 问题1：放电状态下，电芯电压处于过充时，BQ会自动关闭充电MOS管;   解决方案：放电状态下，重新设置硬件过充电压为4.3V
    771                  Afe_OV_UV_Threshold_Set(4300, UV_THREHOLD_VAL_SET); 
    772                }
    773              }
    774            }  
    775            else// if((Check_Val >= 130 && Check_Val < 160) || CC_Val >= 10)
    776            {  
    777              if(ChgExchangeMode_Cnt >= 30)
    778              { 
    779                ChgExchangeMode_Cnt  = 100;
    780                DisExchangeMode_Cnt  = 0;
    781                IdleExchangeMode_Cnt = 0;
    782                WorkMode = CHARGE_MODE;
    783                if(ModeChange_Lock != 1)
    784                {
    785                  ModeChange_Lock = 1;
    786                  // 问题2：充电状态下，电芯电压处于过放时，BQ会自动关闭放电MOS管;   解决方案：充电状态下，重新设置硬件过充电压为1.0V
    787                  Afe_OV_UV_Threshold_Set(OV_THREHOLD_VAL_SET, 1500);
    788                }
    789              }
    790            }   
    791          }
