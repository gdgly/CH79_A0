   1111                  Afe_CellBalance_Disable();
   1112                } 
   1113              }
   1114            }
   1115            else if(CellBal_Cntrl_Lock ==1)
   1116            {
   1117                Afe_CellBalance_Disable();
   1118            }
   1119          }
