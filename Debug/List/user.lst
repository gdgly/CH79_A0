   1216                    {
   1217                      LedFlash_t = 0;
   1218                      FlowLedCnt = 0;
   1219                      FlowLed_Finish_Flag = 1;
   1220                    }  
   1221                  }
   1222                }
   1223                else if(Bits_flag.Bit.ChgOv || (Cell_Volt_Max >= 4050 && Cell_Volt_Avg >= 4050)) 
   1224                {
