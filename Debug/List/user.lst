   1041                    {   
   1042                      CellBalance_Cur_Selct |= (0x0001 << i); 
   1043                    }
   1044                    else
   1045                    {
   1046                      CellBalance_Cur_Selct &= ~(0x0001 << i);
   1047                    }
   1048                  }  
   1049                  CellBalance_Selct = 0x0000;
   1050                  //===================================
   1051                  for(i =0; i <10; i++ )
