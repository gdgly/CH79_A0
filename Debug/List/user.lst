   1892          void SOC_Init(void)
   1893          {
   1894            uint8_t result = 0;
   1895            uint8_t Soc_Tmp = 0;
   1896            result = FLASH_ReadByte(ADJUST_ADDR);
   1897            
   1898            if(result == 0xAA)//判断校验地址中数据是否为0xAA, Y: EEPROM 中保存有SOC数据;  N: EEPROM 中保存有SOC数据
   1899            {
   1900              FLASH_Unlock(FLASH_MEMTYPE_DATA);       // 解锁EEPROM
   1901              FLASH_ProgramByte(ADJUST_ADDR,0x00);    // 清除校验地址中数据 
   1902              FLASH_Lock(FLASH_MEMTYPE_DATA);         // 加锁EEPROM
   1903              Soc_Tmp = FLASH_ReadByte(SOC_ADDR);     // 读取SOC数据
   1904               
   1905              SocReg.soc = Soc_Tmp; //SocReg.ah = SocCalc.curAh; // 计算SOC。 
   1906              SocCalc.curAh = ((uint32_t)SocReg.rated_cap * Soc_Tmp) / 100;
   1907              SocReg.ah = SocCalc.curAh;
   1908              SocCalc.soc_rt = SocReg.soc;
   1909              Soc_OCV_CorrectEn_Flag = 0;  // 上电禁止SOC的OCV校准  
   1910            }
   1911            else
   1912            {
   1913              Soc_OCV_CorrectEn_Flag = 1;  // 上电允许SOC的OCV校准
   1914            } 
   1915          }
   1916          void SOC_SavedtoEEPROM(void)
   1917          {   
   1918            FLASH_Unlock(FLASH_MEMTYPE_DATA);               // 解锁EEPROM
   1919            FLASH_ProgramByte(SOC_ADDR,SocCalc.soc_rt);     // 保存SOC到EEPROM 
   1920            FLASH_Lock(FLASH_MEMTYPE_DATA);                 // 加锁EEPROM
   1921            if(SocCalc.soc_rt == FLASH_ReadByte(SOC_ADDR))  // 读取数据 ?= 写入数据
   1922            {
   1923              FLASH_Unlock(FLASH_MEMTYPE_DATA);             // 解锁EEPROM
   1924              FLASH_ProgramByte(ADJUST_ADDR,0xAA);          // 写入保存SOC成功标志符
   1925              FLASH_Lock(FLASH_MEMTYPE_DATA);               // 加锁EEPROM
   1926            } 
   1927          }
