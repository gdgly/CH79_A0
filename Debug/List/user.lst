    901          //==========================================================================
    902            /*The ADC transfer function is a linear equation defined as follows:
    903              V(cell) = GAIN x ADC(cell) + OFFSET
    904              If the DEVICE_XREADY is set, the voltage register values from the device should not be used. If
    905            */ 
    906          void Afe_Volt_Val_Get(void)
    907          { 
    908            uint8_t i = 0;
    909            uint16_t volt_sum = 0;
    910            uint16_t volt_max = 0;
    911            uint16_t volt_min = 0xFFFF;
    912            //====================================250mS
    913            if(Cell_Volt_Sample_Cnt >= 26)
    914            {
    915              Cell_Volt_Sample_Cnt = 0;
    916              for(i =0;i< 20;i += 2)
    917              {  
    918                Adc_value[i/2] = Afe_Get_Adc((VC1_HI_ADDR + i)) & 0x3FFF; //VC1_HI_ADDR =0x0C
    919                Cell_Volt[i/2] = (uint16_t)((uint32_t)ADCGain_Val *Adc_value[i/2]/1000) + ADCOffset_Val;
    920                volt_sum += Cell_Volt[i/2];
    921                if(Cell_Volt[i/2] < volt_min)
    922                {
    923                   volt_min = Cell_Volt[i/2];
    924                }
    925                if(Cell_Volt[i/2] > volt_max)
    926                {
    927                   volt_max = Cell_Volt[i/2];
    928                }
    929              } 
    930              Cell_Volt_Tol = volt_sum; 
    931              Cell_Volt_Avg = Cell_Volt_Tol/10; 
    932              Cell_Volt_Max = volt_max; 
    933              Cell_Volt_Min = volt_min; 
    934                
    935            }
    936            //====================================250mS
    937            /*  Once converted to digital form, each cell voltage is added up and the summation result stored in the BAT
    938                registers. This 16-bit value has a nominal LSB of 1.532 mV. */
    939            //Pack_Volt = (uint16_t)((uint32_t)4 *ADCGain_Val *Afe_Get_Adc(BAT_HI_ADDR)/1000+(10 *ADCOffset_Val));
    940            //Pack_Volt = (uint16_t)((uint32_t)1532 *Afe_Get_Adc(BAT_HI_ADDR)/1000);
    941            
    942            //====================================250mS
    943            //Positive values are charge current; negative values are discharge current.
    944            //an average over the 250-ms integration period.
    945            /*Why does the CC value read not change when I set the CC_ONESHOT bit?
    946                  The CC_READY bit must be cleared before the CC_ONESHOT bit is set. Note also that the CC_EN bit
    947                  should be cleared or a CC conversion may already be in progress.
    948              The full scale range of the CC is ± 270 mV, with a max recommended input range of ± 200 mV, thus yielding an LSB of approximately 8.44 μV.
    949              The following equation shows how to convert the 16-bit CC reading into an analog voltage if no boardlevel calibration is performed:
    950              CC Reading (in μV) = [16-bit 2’s Complement Value] × (8.44 μV/LSB) 
    951            */
    952            if( SYS_STAT.Bit.CC_READY) //CC_Volt_Sample_Cnt >= 23 &&
    953            {
    954              CC_Volt_Sample_Cnt = 0;
    955              SYS_STAT.Bit.CC_READY = 0;
    956              CC_AD = Afe_Get_Adc(CC_HI_ADDR); 
    957              //CC_Val = (int32_t)CC_AD * 820/100; //mA (int32_t)
    958              CC_Val = (int32_t)820 * CC_AD /100; //mA (int32_t)
    959              //SYS_STAT_Last |= 0x80;
    960              //I2C_Write(SYS_STAT_ADDR,SYS_STAT_Last); 
    961              //SYS_STAT_Last &= ~0x80;
    962              Afe_CC_1Shot_Set();
    963              if(CC_Val < -32000)
    964              {
    965                CC_Val = -32000;
    966              }
    967              else if(CC_Val > 32000)
    968              {
    969                CC_Val = 32000;
    970              }
    971              Current_Val = CC_Val;
    972              if(CC_Val < 0)
    973              {
    974                Current_Val = -CC_Val;
    975              }
    976            }
    977            //====================================2S 
    978            if(Temp_Volt_Sample_Cnt >= 210)
    979            {
    980              //V_TS2_Val = (uint16_t)((uint32_t)(382 * ((uint16_t)adcval <<8 | tmpval))/1000);//mV
    981              V_TS2_Val = Afe_Get_Adc(TS2_HI_ADDR) &0x3FFF ;
    982              V_TS2_Val = (uint16_t)(((uint32_t)382 * V_TS2_Val /1000));  //mV
    983              V_TS2_Val = (uint16_t)(((uint32_t)1024 * V_TS2_Val /3300));  //mV
    984              //R_TS2_Val = (uint16_t)((uint32_t)10000 * V_TS2_Val)/(3300 - V_TS2_Val); 
    985            }
    986            //=====================================================================================
    987            
    988          }
