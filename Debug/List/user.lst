   1603          /*
   1604          In a single-byte write transaction, the CRC is calculated over the slave address, register address, and data.
   1605          */
   1606          void I2C_Write(uint8_t addr,uint8_t data )
   1607          {     
   1608            uint8_t Write_Retry_Cnt = 5,CRC_data = 0;
   1609            uint16_t OverTimeDelay = OverTimeDelay_SET;
   1610            Buf[0] = SLAVE_ADDR <<1 ;
   1611            Buf[1] = addr;
   1612            Buf[2] = data;
   1613            CRC_data = CRC8_Caculate(Buf,3);//slave_addr + reg_addr + data + CRC
   1614            I2C_COM_ERROR_Flag =0;
   1615            
   1616            while(Write_Retry_Cnt > 0)
   1617            { 
   1618              disableInterrupts();
   1619              I2C_CR2 |= 0x04;
   1620              //以下见stm8s中文数据手册P251（图96主设备发送模式发送序列图） 
   1621              //=============================发送起始位 I2C_Start() 
   1622              while(I2C->SR3 & 0x02) {if((OverTimeDelay --) == 0){OverTimeDelay = 500;I2C_COM_ERROR_Flag = 1;break;}}//   // 等待总线空闲   检测i2c-SR3 busy位
   1623              //I2C_CR2 &= ~0x04;        
   1624              I2C->CR2 |= 0x01;        // 产生起始位     CR2 start位    
   1625              //=============================发送器件地址 I2C_SendDAdr(0xD0)
   1626              //EV5：SB=1，读SR1 然后将地址写入DR寄存器将清除该标志。  
   1627              while(!(I2C->SR1 & 0x01));// {if((OverTimeDelay --) == 0){OverTimeDelay = 50;I2C_COM_ERROR_Flag = 1;break;}} //等待START发送完 E5  
   1628              
   1629              I2C->DR = (SLAVE_ADDR <<1) + 0x00;  //发送 器件地址(最后一位是0,表示发送) 
   1630              
   1631              OverTimeDelay = 100; while((OverTimeDelay --) >0);OverTimeDelay = OverTimeDelay_SET;
   1632              //while(!(I2C->SR1 & 0x02 == 0x02)) {if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;I2C_COM_ERROR_Flag = 0;break;}} // //等特7位器件地址发送完并且收到ack,ADDR置1    
   1633              //EV6:ADDR 在软件读取SR1后，对SR3寄存器读操作 将清除该位 
   1634              I2C->SR1;   
   1635              I2C->SR3; //然后读SR3 清  ADDR 
   1636              if(I2C_COM_ERROR_Flag == 0)
   1637              {
   1638                //I2C_CR2 |= 0x04;
   1639                //=============================DATA 发送寄存器地址 I2C_SendDat()  
   1640                I2C->DR = (u8)(addr);      
   1641                //EV8_2 TxE=1 ，BTF=1，产生停止条件时由硬件清除。   
   1642                while(!(I2C->SR1 & 0x84));//84//0x80  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1643                 
   1644                //============================= 发送数据  
   1645                I2C->DR = data;  //发送MLX90615器件地址(最后一位是1,表示接收)，发送完后自动发送ack(提前是CR2 ack位使能)    
   1646                while(!(I2C->SR1 & 0x84));  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1647                //EV8_2：TxE=1，BTF=1，产生停止条件时由硬件清除。  
   1648                
   1649                //============================= 发送数据  
   1650                I2C->DR = CRC_data;    
   1651                while(!(I2C->SR1 & 0x84));  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1652                //EV8_2：TxE=1，BTF=1，产生停止条件时由硬件清除。  
   1653                
   1654                I2C->DR;  //清除I2C_SR2的BTF位
   1655                I2C->CR2 |= 0x02;  //停止位产生stop  //*注意： 发送停止位时，必须先清除I2C_SR2的BTF位  
   1656                
   1657                enableInterrupts();
   1658                OverTimeDelay = 200; while((OverTimeDelay --) >0);
   1659                if(I2C_COM_ERROR_Flag == 0)
   1660                {  
   1661                  return;
   1662                }
   1663              }
   1664              I2C_COM_ERROR_Flag = 0;
   1665              Write_Retry_Cnt -= 1; 
   1666            } 
   1667            enableInterrupts();
   1668            //Uart_SendByte(0xCC);
   1669            //Uart_SendByte(0xCC);
   1670          }
