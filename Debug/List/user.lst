   1839          void SOC_Init(void)
   1840          {
   1841            uint8_t result = 0;
   1842            uint8_t Soc_Tmp = 0;
   1843            result = FLASH_ReadByte(ADJUST_ADDR);
   1844            
   1845            if(result == 0xAA)//判断校验地址中数据是否为0xAA, Y: EEPROM 中保存有SOC数据;  N: EEPROM 中保存有SOC数据
   1846            {
   1847              FLASH_Unlock(FLASH_MEMTYPE_DATA);       // 解锁EEPROM
   1848              FLASH_ProgramByte(ADJUST_ADDR,0x00);    // 清除校验地址中数据 
   1849              FLASH_Lock(FLASH_MEMTYPE_DATA);         // 加锁EEPROM
   1850              Soc_Tmp = FLASH_ReadByte(SOC_ADDR);     // 读取SOC数据
   1851               
   1852              SocReg.soc = Soc_Tmp; //SocReg.ah = SocCalc.curAh; // 计算SOC。 
   1853              SocCalc.curAh = ((uint32_t)SocReg.rated_cap * Soc_Tmp) / 100;
   1854              SocReg.ah = SocCalc.curAh;
   1855              SocCalc.soc_rt = SocReg.soc;
