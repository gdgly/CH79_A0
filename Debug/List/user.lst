    706            }   
    707            else if((Check_Val >= 220 && Check_Val < 630) || SYS_CTRL1.Bit.LOAD_PRESENT || CC_Val < (-10) )
    708            {  
    709              if(DisExchangeMode_Cnt >= 30)
    710              { 
    711                IdleExchangeMode_Cnt = 0;
    712                ChgExchangeMode_Cnt = 0;
    713                DisExchangeMode_Cnt = 100; 
    714                WorkMode = DISCHARGE_MODE;
    715              }
    716            } 
    717            else if(Check_Val < 10)
    718            { 
    719              if(IdleExchangeMode_Cnt >= 100)
    720              { 
    721                IdleExchangeMode_Cnt = 100;
    722                ChgExchangeMode_Cnt = 0;
    723                DisExchangeMode_Cnt = 0;
    724                WorkMode = IDLE_MODE;
    725              }
    726            } 
