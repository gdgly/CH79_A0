    266          void itoa(char *buf, int i, int base)
    267          {
    268              #define LEN	20
    269              char *s;
    270              int rem;
    271              static char rev[LEN+1];
    272            
    273              rev[LEN] = 0;
    274              if (i == 0)
    275              {
    276                (buf)[0] = '0';
    277                ++(buf);
    278                return;
    279              }
    280              s = &rev[LEN];
    281              while (i)
    282              {
    283                rem = i % base;
    284                if (rem < 10)
    285                {
    286                  *--s = rem + '0';
    287                }
    288                else if (base == 16)
    289                {
    290                  *--s = "abcdef"[rem - 10];
    291                }
    292                i /= base;
    293              }
    294              while (*s)
    295              {
    296                (buf)[0] = *s++;
    297                ++(buf);
    298              }
    299          }
