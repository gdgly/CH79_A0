   1502          uint8_t CRC8_Caculate(uint8_t *ptr,uint8_t len)
   1503          {
   1504            uint8_t i;
   1505          	
   1506            uint8_t crc = 0;
   1507          
   1508            uint8_t key = 0x07; //_Poly_07
   1509            while((len--) != 0)
   1510          	
   1511            {
   1512          		
   1513              for(i = 0x80; i != 0; i /= 2)
   1514          		
   1515              {
   1516          			
   1517                if((crc & 0x80) != 0)
   1518          			
   1519                {
   1520          				
   1521                  crc *= 2;
   1522          				
   1523                  crc ^= key;
   1524          			
   1525                }
   1526          			
   1527                else
   1528          				
   1529                {
   1530                  crc *= 2;
   1531          
   1532                }
   1533          			
   1534                if((*ptr & i) != 0)
   1535          				
   1536                {
   1537                  crc ^= key;
   1538                }
   1539          		
   1540              }
   1541          		
   1542              ptr++;
   1543          	
   1544            }
   1545            return (crc);
   1546          
