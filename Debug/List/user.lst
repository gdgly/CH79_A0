    687          void ModeCheck_Backup(void)
    688          {       
    689            //工作模式的检测是依据外部Triger端的电压大小来判定
    690            Check_Val = ADConverse(3); 
    691            if((Check_Val >= 130 && Check_Val < 160) || CC_Val >= 10)
    692            {  
    693              if(ChgExchangeMode_Cnt >= 30)
    694              { 
    695                ChgExchangeMode_Cnt  = 100;
    696                DisExchangeMode_Cnt  = 0;
    697                IdleExchangeMode_Cnt = 0;
    698                WorkMode = CHARGE_MODE;
    699              }
    700            }   
    701            else if((Check_Val >= 220 && Check_Val < 630) || SYS_CTRL1.Bit.LOAD_PRESENT || CC_Val < (-10) )
    702            {  
    703              if(DisExchangeMode_Cnt >= 30)
    704              { 
    705                IdleExchangeMode_Cnt = 0;
    706                ChgExchangeMode_Cnt = 0;
    707                DisExchangeMode_Cnt = 100; 
    708                WorkMode = DISCHARGE_MODE;
    709              }
    710            } 
    711            else if(Check_Val < 10)
    712            { 
    713              if(IdleExchangeMode_Cnt >= 100)
    714              { 
    715                IdleExchangeMode_Cnt = 100;
    716                ChgExchangeMode_Cnt = 0;
    717                DisExchangeMode_Cnt = 0;
    718                WorkMode = IDLE_MODE;
    719              }
    720            } 
    721          }
