    387              TIM4_PSCR = 0x07;       // 计数器时钟=主时钟/128=16MHZ/128  // 相当于计数器周期为8uS 
    388              TIM4_CR1 = 0x15;        // TIM4_ARR寄存器通过缓冲预装载，使能计数器
    389              TIM4_CNTR = 0;          //     
    390              TIM4_ARR = 125;         // 125*8us = 1ms  
