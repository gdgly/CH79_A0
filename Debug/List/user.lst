    606              }
    607              
    608              if(Bits_flag.Bit.ChgOv && Cell_Volt_Max < CHG_OV_RE_VAL_SET)// Cell_Volt_Avg < CHG_OV_RE_VAL_SET &&
    609              {
    610                //clear the OV bit by writing "1"
    611                if(SYS_STAT.Bit.OV)
    612                {
    613                  SYS_STAT_Last |= 0x04;
    614                  I2C_Write(SYS_STAT_ADDR,SYS_STAT_Last);
