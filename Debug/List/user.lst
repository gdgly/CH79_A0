   1498          /*
   1499          In a single-byte read transaction, the CRC is calculated after the second start and uses the slave address and data byte.
   1500          */
   1501          void I2C_Read(uint8_t addr,uint8_t *data)//slave device address 0x18
   1502          {      
   1503            uint16_t OverTimeDelay = OverTimeDelay_SET;
   1504            uint8_t CRC_ReData,Retry_Cnt = 5; 
   1505            Buf[0] = SLAVE_ADDR; 
   1506            while(Retry_Cnt > 0)
   1507            {
   1508              //以下见stm8s中文数据手册P251（图96主设备发送模式发送序列图）  
   1509              disableInterrupts(); 
   1510              I2C->CR2 |= 0x04; //ack使能 
   1511              //=============================发送起始位 I2C_Start() 
   1512              while(I2C->SR3 & 0x02){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;I2C_COM_ERROR_Flag = 1;break;}}    // 等待总线空闲   检测i2c-SR3 busy位        
   1513              I2C->CR2 |= 0x01;        // 产生起始位     CR2 start位  
   1514              
   1515              //=============================发送器件地址 I2C_SendDAdr(0xD0)
   1516              //EV5：SB=1，读SR1 然后将地址写入DR寄存器将清除该标志。  
   1517              while(!(I2C->SR1 & 0x01));  //等待START发送完 E5  
   1518              I2C->DR = (SLAVE_ADDR << 1) + 0x00;  //发送 器件地址(最后一位是0,表示发送) 
   1519              
   1520              while(!(I2C->SR1 & 0x02 == 0x02)){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;break;}}// I2C_COM_ERROR_Flag = 1; //等特7位器件地址发送完并且收到ack,ADDR置1    
   1521              //while(!(I2C->SR1 & 0x02));  //等特7位器件地址发送完并且收到ack,ADDR置1    
   1522              //EV6:ADDR 在软件读取SR1后，对SR3寄存器读操作 将清除该位 
   1523              I2C->SR1; //见P251 读SR1 (实验证明可以不要)  
   1524              I2C->SR3; //然后读SR3 清  ADDR 
   1525              
   1526              if(I2C_COM_ERROR_Flag == 0)
   1527              {
