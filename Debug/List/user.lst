    264          void itoa(char *buf, int i, int base)
    265          {
    266              #define LEN	20
    267              char *s;
    268              int rem;
    269              static char rev[LEN+1];
    270            
    271              rev[LEN] = 0;
    272              if (i == 0)
    273              {
    274                (buf)[0] = '0';
    275                ++(buf);
    276                return;
    277              }
    278              s = &rev[LEN];
    279              while (i)
    280              {
    281                rem = i % base;
    282                if (rem < 10)
    283                {
    284                  *--s = rem + '0';
    285                }
    286                else if (base == 16)
    287                {
    288                  *--s = "abcdef"[rem - 10];
    289                }
    290                i /= base;
    291              }
    292              while (*s)
    293              {
    294                (buf)[0] = *s++;
    295                ++(buf);
    296              }
    297          }
