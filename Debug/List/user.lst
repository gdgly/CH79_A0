   1511          uint8_t CRC8_Caculate(uint8_t *ptr,uint8_t len)
   1512          {
   1513            uint8_t i;
   1514          	
   1515            uint8_t crc = 0;
   1516          
   1517            uint8_t key = 0x07; //_Poly_07
   1518            while((len--) != 0)
   1519          	
   1520            {
   1521          		
   1522              for(i = 0x80; i != 0; i /= 2)
   1523          		
   1524              {
   1525          			
   1526                if((crc & 0x80) != 0)
   1527          			
   1528                {
   1529          				
   1530                  crc *= 2;
   1531          				
   1532                  crc ^= key;
   1533          			
