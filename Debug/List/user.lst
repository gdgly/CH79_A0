    360          void Uart_SendData(unsigned int tx_data, int base) 
    361          {
    362            unsigned char buf[20] = {0};
    363          
    364            itoa((char *)buf, tx_data, base);
    365            
    366            if (base == 16)
    367            {
    368              Uart_SendStr((unsigned char *)"0x");
    369            }
    370            
    371            Uart_SendStr(buf);
    372          }
