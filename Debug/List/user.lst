   1098                    if((CellBalance_Cur_Selct &(0x0001 <<i)))
   1099                    {
   1100                      CellBalance_Selct |=(0x0001 <<i);
   1101                      if(4 != i)
   1102                      {
   1103                        i +=1; 
   1104                      }
   1105                    }  
   1106                  } 
