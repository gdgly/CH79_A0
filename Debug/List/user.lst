   1605          /*
   1606          In a single-byte read transaction, the CRC is calculated after the second start and uses the slave address and data byte.
   1607          */
   1608          void I2C_Read(uint8_t addr,uint8_t *data)//slave device address 0x18
   1609          {      
   1610            uint16_t OverTimeDelay = OverTimeDelay_SET;
   1611            uint8_t CRC_ReData,Retry_Cnt = 5; 
   1612            Buf[0] = SLAVE_ADDR; 
   1613            while(Retry_Cnt > 0)
   1614            {
   1615              //以下见stm8s中文数据手册P251（图96主设备发送模式发送序列图）  
   1616              disableInterrupts(); 
   1617              I2C->CR2 |= 0x04; //ack使能 
   1618              //=============================发送起始位 I2C_Start() 
   1619              while(I2C->SR3 & 0x02){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;I2C_COM_ERROR_Flag = 1;break;}}    // 等待总线空闲   检测i2c-SR3 busy位        
   1620              I2C->CR2 |= 0x01;        // 产生起始位     CR2 start位  
   1621              
   1622              //=============================发送器件地址 I2C_SendDAdr(0xD0)
   1623              //EV5：SB=1，读SR1 然后将地址写入DR寄存器将清除该标志。  
   1624              while(!(I2C->SR1 & 0x01));  //等待START发送完 E5  
   1625              I2C->DR = (SLAVE_ADDR << 1) + 0x00;  //发送 器件地址(最后一位是0,表示发送) 
   1626              
   1627              while(!(I2C->SR1 & 0x02 == 0x02)){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;break;}}// I2C_COM_ERROR_Flag = 1; //等特7位器件地址发送完并且收到ack,ADDR置1    
   1628              //while(!(I2C->SR1 & 0x02));  //等特7位器件地址发送完并且收到ack,ADDR置1    
   1629              //EV6:ADDR 在软件读取SR1后，对SR3寄存器读操作 将清除该位 
   1630              I2C->SR1; //见P251 读SR1 (实验证明可以不要)  
   1631              I2C->SR3; //然后读SR3 清  ADDR 
   1632              
   1633              if(I2C_COM_ERROR_Flag == 0)
   1634              {
   1635                //=============================DATA 发送寄存器地址 I2C_SendDat()  
   1636                I2C->DR = (u8)(addr);      
   1637                //EV8_2 TxE=1 ，BTF=1，产生停止条件时由硬件清除。   
   1638                while(!(I2C->SR1 & 0x84));  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1639                //在发送地址和清除ADDR 之后，I2C接口进入主设备接收模式。以下见stm8s中文数据手册P252（图97主设备接收模式接收序列图）  
   1640                
   1641                //=============================I2C_Start()        
   1642                I2C->CR2 |= 0x01;  //产生重复起始位   
   1643                //EV5：SB=1，读SR1 然后将地址写入DR寄存器将清除该标志。  
   1644                while(!(I2C->SR1 & 0x01));  //等待START发送完     
   1645                //ADDRESS (接收)   
   1646                I2C->DR = (SLAVE_ADDR <<1) + 0x01;  //发送MLX90615器件地址(最后一位是1,表示接收)，发送完后自动发送ack(提前是CR2 ack位使能)      
   1647                //EV6:ADDR 在软件读取SR1后，对SR3寄存器读操作 将清除改位   
   1648                
   1649                while(!(I2C->SR1 & 0x02 == 0x02)){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;break;}} // I2C_COM_ERROR_Flag = 1;//等特7位器件地址发送完并且收到ack,ADDR置1    
   1650                //while(!(I2C->SR1 & 0x02));  //等特7位器件地址发送完并且收到ack,ADDR置1  
   1651                I2C->SR1; //见P251 读SR1 (实验证明可以不要)  
   1652                I2C->SR3; //然后读SR3 清  ADDR   
   1653                   Buf[0] = 0x31;  Buf[1] = 0;     Buf[2] = 0;           
   1654                //测试EV7 RxNE=1（收到一个字节后RxNE置1） ，判断DR寄存器有数据    
   1655                while(!(I2C->SR1 & 0x40));                                        
   1656                Buf[1] = I2C->DR;//在接收模式下，收到完整字节后,自动发送ack(提前是CR2 ack位使能，不需要专门CR2 ack位置1)                                               
   1657                 
   1658               
   1659                
   1660                //测试EV7 RxNE=1（收到一个字节后RxNE置1） ，判断DR寄存器有数据    
   1661                while(!(I2C->SR1 & 0x40));                                        
   1662                Buf[2] = I2C->DR;//在接收模式下，收到完整字节后,自动发送ack(提前是CR2 ack位使能，不需要专门CR2 ack位置1)  
   1663                  //EV7_1 ：RxNE=1 ，读DR寄存器清除该标志。设置ACK=0和STOP 请求。(在接收最后一个字节前)     
   1664                I2C->SR3;
   1665                I2C->DR; 
   1666                I2C->CR2 &= ~0x04; //ack使能  
   1667                I2C->CR2 |= 0x02;  //停止位产生stop      
   1668                 
   1669                enableInterrupts(); 
   1670                //while((I2C->CR2 & I2C_CR2_STOP));
   1671                CRC_ReData = CRC8_Caculate(Buf,2);
   1672                /*Uart_SendByte(0x11);
   1673                Uart_SendByte(Buf[0]);
   1674                Uart_SendByte(Buf[1]);
   1675                Uart_SendByte(Buf[2]);*/
   1676                if(CRC_ReData == Buf[2])
   1677                {
   1678                  *data = Buf[1];
   1679                  OverTimeDelay = 200; while((OverTimeDelay --) >0);
   1680                  return;
   1681                }
   1682              }
   1683              I2C_COM_ERROR_Flag = 0;
   1684              Retry_Cnt -= 1;
   1685            }
   1686            enableInterrupts(); 
   1687            //Uart_SendByte(0xDD);
   1688            //Uart_SendByte(0xDD);
   1689          }
