    991          /*
    992            turn on condition  :    >4.0V && deltaVolt >200mV
    993            turn off condition :   over charge assert the charging protection
    994          */ 
    995          void CellBal_Cntrl(void)
    996          {
    997            uint8_t i = 0;
    998            if(WorkMode == CHARGE_MODE)
    999            {
   1000              if(Cell_Balance_Delay_t >= 100)
   1001              {
   1002                Cell_Balance_Delay_t = 0;
   1003                if((Cell_Volt_Max >= CELLBALANCE_BEGIN_VAL) && ((Cell_Volt_Max - Cell_Volt_Min) >= CELLBALANCE_DELTA_VAL) && !Bits_flag.Bit.ChgOv && !Bits_flag.Bit.ChgTemp && !Bits_flag.Bit.ChgCurOv)
   1004                {
   1005                  CellBal_Cntrl_Lock = 1;
   1006                  for(i =0; i <10; i++)
   1007                  {
   1008                    if(Cell_Volt[i] > CELLBALANCE_BEGIN_VAL && ((Cell_Volt[i] - Cell_Volt_Min) >= CELLBALANCE_DELTA_VAL))
   1009                    {   
   1010                      CellBalance_Cur_Selct |= (0x0001 << i); 
   1011                    }
   1012                    else
   1013                    {
   1014                      CellBalance_Cur_Selct &= ~(0x0001 << i);
   1015                    }
   1016                  }  
   1017                  CellBalance_Selct = 0x0000;
   1018                  //===================================
   1019                  for(i =0; i <10; i++ )
   1020                  {
   1021                    if((CellBalance_Cur_Selct &(0x0001 <<i)))
   1022                    {
   1023                      CellBalance_Selct |=(0x0001 <<i);
   1024                      if(4 != i)
   1025                      {
   1026                        i +=1; 
   1027                      }
   1028                    }  
   1029                  } 
   1030                  Afe_CellBalance_Enable(CellBalance_Selct);
   1031                } 
   1032                else if(CellBal_Cntrl_Lock ==1)
   1033                { 
   1034                  Afe_CellBalance_Disable();
   1035                } 
   1036              }
   1037            }
   1038            else if(CellBal_Cntrl_Lock ==1)
   1039            {
   1040                Afe_CellBalance_Disable();
   1041            }
   1042          }
