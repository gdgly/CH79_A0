   1604          /*
   1605          In a single-byte read transaction, the CRC is calculated after the second start and uses the slave address and data byte.
   1606          */
   1607          void I2C_Read(uint8_t addr,uint8_t *data)//slave device address 0x18
   1608          {      
   1609            uint16_t OverTimeDelay = OverTimeDelay_SET;
   1610            uint8_t CRC_ReData,Retry_Cnt = 5; 
   1611            Buf[0] = SLAVE_ADDR; 
   1612            while(Retry_Cnt > 0)
   1613            {
   1614              //以下见stm8s中文数据手册P251（图96主设备发送模式发送序列图）  
   1615              disableInterrupts(); 
   1616              I2C->CR2 |= 0x04; //ack使能 
   1617              //=============================发送起始位 I2C_Start() 
   1618              while(I2C->SR3 & 0x02){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;I2C_COM_ERROR_Flag = 1;break;}}    // 等待总线空闲   检测i2c-SR3 busy位        
   1619              I2C->CR2 |= 0x01;        // 产生起始位     CR2 start位  
   1620              
   1621              //=============================发送器件地址 I2C_SendDAdr(0xD0)
   1622              //EV5：SB=1，读SR1 然后将地址写入DR寄存器将清除该标志。  
   1623              while(!(I2C->SR1 & 0x01));  //等待START发送完 E5  
   1624              I2C->DR = (SLAVE_ADDR << 1) + 0x00;  //发送 器件地址(最后一位是0,表示发送) 
   1625              
   1626              while(!(I2C->SR1 & 0x02 == 0x02)){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;break;}}// I2C_COM_ERROR_Flag = 1; //等特7位器件地址发送完并且收到ack,ADDR置1    
   1627              //while(!(I2C->SR1 & 0x02));  //等特7位器件地址发送完并且收到ack,ADDR置1    
   1628              //EV6:ADDR 在软件读取SR1后，对SR3寄存器读操作 将清除该位 
   1629              I2C->SR1; //见P251 读SR1 (实验证明可以不要)  
   1630              I2C->SR3; //然后读SR3 清  ADDR 
   1631              
   1632              if(I2C_COM_ERROR_Flag == 0)
   1633              {
   1634                //=============================DATA 发送寄存器地址 I2C_SendDat()  
   1635                I2C->DR = (u8)(addr);      
   1636                //EV8_2 TxE=1 ，BTF=1，产生停止条件时由硬件清除。   
   1637                while(!(I2C->SR1 & 0x84));  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1638                //在发送地址和清除ADDR 之后，I2C接口进入主设备接收模式。以下见stm8s中文数据手册P252（图97主设备接收模式接收序列图）  
   1639                
   1640                //=============================I2C_Start()        
   1641                I2C->CR2 |= 0x01;  //产生重复起始位   
   1642                //EV5：SB=1，读SR1 然后将地址写入DR寄存器将清除该标志。  
   1643                while(!(I2C->SR1 & 0x01));  //等待START发送完     
   1644                //ADDRESS (接收)   
   1645                I2C->DR = (SLAVE_ADDR <<1) + 0x01;  //发送MLX90615器件地址(最后一位是1,表示接收)，发送完后自动发送ack(提前是CR2 ack位使能)      
   1646                //EV6:ADDR 在软件读取SR1后，对SR3寄存器读操作 将清除改位   
   1647                
   1648                while(!(I2C->SR1 & 0x02 == 0x02)){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;break;}} // I2C_COM_ERROR_Flag = 1;//等特7位器件地址发送完并且收到ack,ADDR置1    
   1649                //while(!(I2C->SR1 & 0x02));  //等特7位器件地址发送完并且收到ack,ADDR置1  
   1650                I2C->SR1; //见P251 读SR1 (实验证明可以不要)  
   1651                I2C->SR3; //然后读SR3 清  ADDR   
   1652                   Buf[0] = 0x31;  Buf[1] = 0;     Buf[2] = 0;           
   1653                //测试EV7 RxNE=1（收到一个字节后RxNE置1） ，判断DR寄存器有数据    
   1654                while(!(I2C->SR1 & 0x40));                                        
   1655                Buf[1] = I2C->DR;//在接收模式下，收到完整字节后,自动发送ack(提前是CR2 ack位使能，不需要专门CR2 ack位置1)                                               
   1656                 
   1657               
   1658                
   1659                //测试EV7 RxNE=1（收到一个字节后RxNE置1） ，判断DR寄存器有数据    
   1660                while(!(I2C->SR1 & 0x40));                                        
   1661                Buf[2] = I2C->DR;//在接收模式下，收到完整字节后,自动发送ack(提前是CR2 ack位使能，不需要专门CR2 ack位置1)  
   1662                  //EV7_1 ：RxNE=1 ，读DR寄存器清除该标志。设置ACK=0和STOP 请求。(在接收最后一个字节前)     
   1663                I2C->SR3;
   1664                I2C->DR; 
   1665                I2C->CR2 &= ~0x04; //ack使能  
   1666                I2C->CR2 |= 0x02;  //停止位产生stop      
   1667                 
   1668                enableInterrupts(); 
   1669                //while((I2C->CR2 & I2C_CR2_STOP));
   1670                CRC_ReData = CRC8_Caculate(Buf,2);
   1671                /*Uart_SendByte(0x11);
   1672                Uart_SendByte(Buf[0]);
   1673                Uart_SendByte(Buf[1]);
   1674                Uart_SendByte(Buf[2]);*/
   1675                if(CRC_ReData == Buf[2])
   1676                {
   1677                  *data = Buf[1];
   1678                  OverTimeDelay = 200; while((OverTimeDelay --) >0);
   1679                  return;
   1680                }
   1681              }
   1682              I2C_COM_ERROR_Flag = 0;
   1683              Retry_Cnt -= 1;
   1684            }
   1685            enableInterrupts(); 
   1686            //Uart_SendByte(0xDD);
   1687            //Uart_SendByte(0xDD);
   1688          }
