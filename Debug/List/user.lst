    160          void ClrWdt(void)
    161          {
    162              IWDG_KR = 0x55;   //解除保护
    163              IWDG_RLR = 0xFF;  //刷新内容
    164              IWDG_PR = 6; 
    165              IWDG_KR = 0xAA;   //刷新及恢复保护
    166              IWDG_KR = 0xCC;   //独立看门狗启动
    167          } 
