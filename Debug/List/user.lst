   1585          /*
   1586          In a single-byte write transaction, the CRC is calculated over the slave address, register address, and data.
   1587          */
   1588          void I2C_Write(uint8_t addr,uint8_t data )
   1589          {     
   1590            uint8_t Write_Retry_Cnt = 5,CRC_data = 0;
   1591            uint16_t OverTimeDelay = OverTimeDelay_SET;
   1592            Buf[0] = SLAVE_ADDR <<1 ;
   1593            Buf[1] = addr;
   1594            Buf[2] = data;
   1595            CRC_data = CRC8_Caculate(Buf,3);//slave_addr + reg_addr + data + CRC
   1596            I2C_COM_ERROR_Flag =0;
   1597            
   1598            while(Write_Retry_Cnt > 0)
   1599            { 
   1600              disableInterrupts();
   1601              I2C_CR2 |= 0x04;
   1602              //以下见stm8s中文数据手册P251（图96主设备发送模式发送序列图） 
   1603              //=============================发送起始位 I2C_Start() 
   1604              while(I2C->SR3 & 0x02) {if((OverTimeDelay --) == 0){OverTimeDelay = 500;I2C_COM_ERROR_Flag = 1;break;}}//   // 等待总线空闲   检测i2c-SR3 busy位
   1605              //I2C_CR2 &= ~0x04;        
   1606              I2C->CR2 |= 0x01;        // 产生起始位     CR2 start位    
   1607              //=============================发送器件地址 I2C_SendDAdr(0xD0)
   1608              //EV5：SB=1，读SR1 然后将地址写入DR寄存器将清除该标志。  
   1609              while(!(I2C->SR1 & 0x01));// {if((OverTimeDelay --) == 0){OverTimeDelay = 50;I2C_COM_ERROR_Flag = 1;break;}} //等待START发送完 E5  
   1610              
   1611              I2C->DR = (SLAVE_ADDR <<1) + 0x00;  //发送 器件地址(最后一位是0,表示发送) 
   1612              
   1613              OverTimeDelay = 100; while((OverTimeDelay --) >0);OverTimeDelay = OverTimeDelay_SET;
   1614              //while(!(I2C->SR1 & 0x02 == 0x02)) {if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;I2C_COM_ERROR_Flag = 0;break;}} // //等特7位器件地址发送完并且收到ack,ADDR置1    
   1615              //EV6:ADDR 在软件读取SR1后，对SR3寄存器读操作 将清除该位 
   1616              I2C->SR1;   
   1617              I2C->SR3; //然后读SR3 清  ADDR 
   1618              if(I2C_COM_ERROR_Flag == 0)
   1619              {
   1620                //I2C_CR2 |= 0x04;
   1621                //=============================DATA 发送寄存器地址 I2C_SendDat()  
   1622                I2C->DR = (u8)(addr);      
   1623                //EV8_2 TxE=1 ，BTF=1，产生停止条件时由硬件清除。   
   1624                while(!(I2C->SR1 & 0x84));//84//0x80  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1625                 
   1626                //============================= 发送数据  
   1627                I2C->DR = data;  //发送MLX90615器件地址(最后一位是1,表示接收)，发送完后自动发送ack(提前是CR2 ack位使能)    
   1628                while(!(I2C->SR1 & 0x84));  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1629                //EV8_2：TxE=1，BTF=1，产生停止条件时由硬件清除。  
   1630                
   1631                //============================= 发送数据  
   1632                I2C->DR = CRC_data;    
   1633                while(!(I2C->SR1 & 0x84));  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1634                //EV8_2：TxE=1，BTF=1，产生停止条件时由硬件清除。  
   1635                
   1636                I2C->DR;  //清除I2C_SR2的BTF位
   1637                I2C->CR2 |= 0x02;  //停止位产生stop  //*注意： 发送停止位时，必须先清除I2C_SR2的BTF位  
   1638                
   1639                enableInterrupts();
   1640                OverTimeDelay = 200; while((OverTimeDelay --) >0);
   1641                if(I2C_COM_ERROR_Flag == 0)
   1642                {  
   1643                  return;
   1644                }
   1645              }
   1646              I2C_COM_ERROR_Flag = 0;
   1647              Write_Retry_Cnt -= 1; 
   1648            } 
   1649            enableInterrupts();
   1650            //Uart_SendByte(0xCC);
   1651            //Uart_SendByte(0xCC);
   1652          }
