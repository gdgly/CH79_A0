    944            /*The ADC transfer function is a linear equation defined as follows:
    945              V(cell) = GAIN x ADC(cell) + OFFSET
    946              If the DEVICE_XREADY is set, the voltage register values from the device should not be used. If
    947            */ 
    948          void Afe_Volt_Val_Get(void)
    949          { 
    950            uint8_t i = 0;
    951            uint16_t volt_sum = 0;
    952            uint16_t volt_max = 0;
    953            uint16_t volt_min = 0xFFFF;
    954            //====================================250mS
    955            if(Cell_Volt_Sample_Cnt >= 26)
    956            {
    957              Cell_Volt_Sample_Cnt = 0;
    958              for(i =0;i< 20;i += 2)
    959              {  
    960                Adc_value[i/2] = Afe_Get_Adc((VC1_HI_ADDR + i)) & 0x3FFF; //VC1_HI_ADDR =0x0C
    961                Cell_Volt[i/2] = (uint16_t)((uint32_t)ADCGain_Val *Adc_value[i/2]/1000) + ADCOffset_Val;
    962                volt_sum += Cell_Volt[i/2];
    963                if(Cell_Volt[i/2] < volt_min)
    964                {
    965                   volt_min = Cell_Volt[i/2];
    966                }
    967                if(Cell_Volt[i/2] > volt_max)
    968                {
    969                   volt_max = Cell_Volt[i/2];
    970                }
    971              } 
    972              Cell_Volt_Tol = volt_sum; 
    973              Cell_Volt_Avg = Cell_Volt_Tol/10; 
    974              Cell_Volt_Max = volt_max; 
    975              Cell_Volt_Min = volt_min; 
    976                
    977            }
    978            //====================================250mS
    979            /*  Once converted to digital form, each cell voltage is added up and the summation result stored in the BAT
    980                registers. This 16-bit value has a nominal LSB of 1.532 mV. */
    981            //Pack_Volt = (uint16_t)((uint32_t)4 *ADCGain_Val *Afe_Get_Adc(BAT_HI_ADDR)/1000+(10 *ADCOffset_Val));
    982            //Pack_Volt = (uint16_t)((uint32_t)1532 *Afe_Get_Adc(BAT_HI_ADDR)/1000);
    983            
    984            //====================================250mS
    985            //Positive values are charge current; negative values are discharge current.
    986            //an average over the 250-ms integration period.
    987            /*Why does the CC value read not change when I set the CC_ONESHOT bit?
    988                  The CC_READY bit must be cleared before the CC_ONESHOT bit is set. Note also that the CC_EN bit
    989                  should be cleared or a CC conversion may already be in progress.
    990              The full scale range of the CC is ± 270 mV, with a max recommended input range of ± 200 mV, thus yielding an LSB of approximately 8.44 μV.
    991              The following equation shows how to convert the 16-bit CC reading into an analog voltage if no boardlevel calibration is performed:
    992              CC Reading (in μV) = [16-bit 2’s Complement Value] × (8.44 μV/LSB) 
    993            */
    994            if((CC_Volt_Sample_Cnt >= 26) && SYS_STAT.Bit.CC_READY) //(0)//
    995            {
    996              CC_Volt_Sample_Cnt = 0;
    997              SYS_STAT.Bit.CC_READY = 0;
    998              CC_AD = Afe_Get_Adc(CC_HI_ADDR);  
    999              //CC_Val = (int32_t)820 * CC_AD /100; //mA (int32_t)
   1000              CC_Val = (int32_t)820 * CC_AD /500; //mA (int32_t)
   1001               
   1002              Afe_CC_1Shot_Set();
   1003              if(CC_Val < -320000)
   1004              {
   1005                CC_Val = -320000;
   1006              }
   1007              else if(CC_Val > 320000)
   1008              {
   1009                CC_Val = 320000;
   1010              }
   1011              Current_Val = CC_Val;
   1012              if(CC_Val < 0)
   1013              {
   1014                Current_Val = -CC_Val;
   1015              }
   1016            }
   1017            //====================================2S 
   1018            if(Temp_Volt_Sample_Cnt >= 210)
   1019            {
   1020              //V_TS2_Val = (uint16_t)((uint32_t)(382 * ((uint16_t)adcval <<8 | tmpval))/1000);//mV
   1021              V_TS2_Val = Afe_Get_Adc(TS2_HI_ADDR) &0x3FFF ;
   1022              V_TS2_Val = (uint16_t)(((uint32_t)382 * V_TS2_Val /1000));  //mV
   1023              V_TS2_Val = (uint16_t)(((uint32_t)1024 * V_TS2_Val /3300));  //mV
   1024              //R_TS2_Val = (uint16_t)((uint32_t)10000 * V_TS2_Val)/(3300 - V_TS2_Val); 
   1025            }
   1026            //=====================================================================================
   1027            
   1028          }
