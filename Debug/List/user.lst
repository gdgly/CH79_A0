    401              CLK_PCKENR1 |= 0x20;    // Fmaster 与外设TIM2连接
    402              TIM2_CR1 = 0x84;        // TIM2_ARR寄存器通过缓冲预装载，使能计数器
    403              TIM2_IER = 0x00;        // 禁止中断
    404              //TIM2_EGR = 0x01;        // 允许产生更新事件 
    405              TIM2_PSCR = 0x07;       // 计数器时钟=主时钟/8=16MHZ/128    8us// 相当于计数器周期为 
    406              TIM2_CNTRH = 0;         //     
    407              TIM2_CNTRL = 0;         //     
    408              TIM2_ARRH = 0x04;       //     10ms
    409              TIM2_ARRL = 0xE2;       //   
    410              TIM2_IER |= 0x01;       // 使能更新中断，禁止触发中断  
    411              TIM2_CR1 |= 0x01;        // TIM2_ARR寄存器通过缓冲预装载，使能计数器
    412          } 
    413          //=======================================================================
    414          //==================================================================
    415          //==================================================================
    416          //--Fmaster = CK_PSC =16MHz, CK_PSC prescaler(128) to CK_CNT(8us)   TIM4_SR1.UIF更新中断标志符
    417          void Timer4_Init_us(void)
    418          {
    419              CLK_PCKENR1 |= 0x10;    //Fmaster 与外设TIM4连接
    420              TIM4_IER = 0x00;        // 禁止中断
    421              TIM4_EGR = 0x01;        // 允许产生更新事件 
    422              //TIM4_PSCR = 0x07;       // 计数器时钟=主时钟/128=16MHZ/128  // 相当于计数器周期为8uS
    423              TIM4_PSCR = 0x00;       // 计数器时钟=主时钟/128=16MHZ/1  // 相当于计数器周期为0.0625uS
    424              TIM4_CR1 = 0x15;        // TIM4_ARR寄存器通过缓冲预装载，使能计数器
    425              TIM4_CNTR = 0;          //     
    426              TIM4_ARR = 16;           // 0.0625*16 = 1us  // 6*8us = 200us  
    427              TIM4_IER |= 0x01;       //更新中断使能，禁止触发中断 
    428              TIM4_CR1 |= 0x01; 
    429          } 
    430          void Timer4_Init_ms(void)
    431          {
    432              CLK_PCKENR1 |= 0x10;    //Fmaster 与外设TIM4连接
    433              TIM4_IER = 0x00;        // 禁止中断
    434              TIM4_EGR = 0x01;        // 允许产生更新事件 
    435              TIM4_PSCR = 0x07;       // 计数器时钟=主时钟/128=16MHZ/128  // 相当于计数器周期为8uS 
    436              TIM4_CR1 = 0x15;        // TIM4_ARR寄存器通过缓冲预装载，使能计数器
    437              TIM4_CNTR = 0;          //     
    438              TIM4_ARR = 125;         // 125*8us = 1ms  
    439              TIM4_IER |= 0x01;       //更新中断使能，禁止触发中断 
    440              TIM4_CR1 |= 0x01; 
    441          } 
    442          void Timer4_Disable(void)
    443          {
    444              TIM4_CR1 &= ~0x01;
    445              TIM4_IER &= ~0x01;
    446              CLK_PCKENR1 &= ~0x10;    //Fmaster 与外设TIM4断开连接 
    447          } 
    448          void Delay_us(uint16_t Delay_time)
    449          {
    450            Delay_time_t = Delay_time;
    451            Timer4_Init_us();
    452            while(Delay_time_t > 0);  
    453            Timer4_Disable();
    454          }
    455          void Delay_ms(uint16_t Delay_time)
    456          {
    457            Delay_time_t = Delay_time;
    458            Timer4_Init_ms();
    459            while(Delay_time_t > 0)
    460            {
    461              if(Delay_time_t >= 300)
    462              {  
    463                ;//ClrWdt(); 
    464              } 
    465            }
    466            Timer4_Disable();
    467          }
