    317          void itoa(char *buf, int i, int base)
    318          {
    319          	#define LEN	20
    320          	char *s;
    321          	int rem;
    322          	static char rev[LEN+1];
    323          
    324          	rev[LEN] = 0;
    325          	if (i == 0)
    326          	{
    327          		(buf)[0] = '0';
    328          		++(buf);
    329          		return;
    330          	}
    331          	s = &rev[LEN];
    332          	while (i)
    333          	{
    334          		rem = i % base;
    335          		if (rem < 10)
    336          			*--s = rem + '0';
    337          		else if (base == 16)
    338          			*--s = "abcdef"[rem - 10];
    339          		i /= base;
    340          	}
    341          	while (*s)
    342          	{
    343          		(buf)[0] = *s++;
    344          		++(buf);
    345          	}
    346          }
