   1845            uint8_t result = 0;
   1846            uint8_t Soc_Tmp = 0;
   1847            result = FLASH_ReadByte(ADJUST_ADDR);
   1848            
   1849            if(result == 0xAA)//判断校验地址中数据是否为0xAA, Y: EEPROM 中保存有SOC数据;  N: EEPROM 中保存有SOC数据
   1850            {
   1851              Soc_Tmp = FLASH_ReadByte(SOC_ADDR);     // 读取SOC数据
   1852              SocReg.soc = Soc_Tmp; //SocReg.ah = SocCalc.curAh; // 计算SOC。 
   1853              SocCalc.curAh = ((uint32_t)SocReg.rated_cap * Soc_Tmp) / 100;
   1854              SocReg.ah = SocCalc.curAh;
   1855              SocCalc.soc_rt = SocReg.soc;
   1856              Soc_OCV_CorrectEn_Flag = 0;  // 上电禁止SOC的OCV校准  
   1857              
   1858              FLASH_Unlock(FLASH_MEMTYPE_DATA);       // 解锁EEPROM
   1859              FLASH_ProgramByte(ADJUST_ADDR,0x00);    // 清除校验地址中数据 
   1860              FLASH_Lock(FLASH_MEMTYPE_DATA);         // 加锁EEPROM
   1861               
   1862              FLASH_Unlock(FLASH_MEMTYPE_DATA);       // 解锁EEPROM
   1863              FLASH_ProgramByte(0x004008,SocCalc.soc_rt);    // 清除校验地址中数据 
   1864              FLASH_Lock(FLASH_MEMTYPE_DATA);         // 加锁EEPROM
   1865               /*
   1866                      FLASH_Unlock(FLASH_MEMTYPE_DATA);       // 解锁EEPROM
   1867                      FLASH_ProgramByte(0x00400C,(uint8_t)SocReg.ah);    // 清除校验地址中数据 
   1868                      FLASH_Lock(FLASH_MEMTYPE_DATA);         // 加锁EEPROM
   1869                      FLASH_Unlock(FLASH_MEMTYPE_DATA);       // 解锁EEPROM
   1870                      FLASH_ProgramByte(0x00400D,(uint8_t)(SocReg.ah>>8));    // 清除校验地址中数据 
   1871                      FLASH_Lock(FLASH_MEMTYPE_DATA);         // 加锁EEPROM
   1872                      FLASH_Unlock(FLASH_MEMTYPE_DATA);       // 解锁EEPROM
   1873                      FLASH_ProgramByte(0x00400E,(uint8_t)(SocReg.ah>>16));    // 清除校验地址中数据 
   1874                      FLASH_Lock(FLASH_MEMTYPE_DATA);         // 加锁EEPROM
   1875                      FLASH_Unlock(FLASH_MEMTYPE_DATA);       // 解锁EEPROM
   1876                      FLASH_ProgramByte(0x00400F,(uint8_t)(SocReg.ah>>24));    // 清除校验地址中数据 
   1877                      FLASH_Lock(FLASH_MEMTYPE_DATA);         // 加锁EEPROM
   1878              */
   1879            }
   1880            else
   1881            {
   1882              Soc_OCV_CorrectEn_Flag = 1;  // 上电允许SOC的OCV校准
   1883            } 
   1884          }
   1885          void SOC_SavedtoEEPROM(void)
   1886          {   
   1887            static uint8_t soc_rt_lock = 0;
   1888            static uint8_t soc_rt_tmp = 0;
   1889            if(soc_rt_lock ==0)
   1890            {
   1891              soc_rt_tmp  = SocCalc.soc_rt;
   1892              soc_rt_lock = 1;
   1893            }
   1894            //soc_rt_tmp  = 80;
   1895            FLASH_Unlock(FLASH_MEMTYPE_DATA);               // 解锁EEPROM
   1896            FLASH_ProgramByte(SOC_ADDR,soc_rt_tmp);     // 保存SOC到EEPROM 
   1897            FLASH_Lock(FLASH_MEMTYPE_DATA);                 // 加锁EEPROM
   1898            
   1899            if(soc_rt_tmp == FLASH_ReadByte(SOC_ADDR))  // 读取数据 ?= 写入数据
   1900            {
   1901              FLASH_Unlock(FLASH_MEMTYPE_DATA);             // 解锁EEPROM
   1902              FLASH_ProgramByte(ADJUST_ADDR,0xAA);          // 写入保存SOC成功标志符
   1903              FLASH_Lock(FLASH_MEMTYPE_DATA);               // 加锁EEPROM
   1904            } 
   1905          }
