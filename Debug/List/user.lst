    303          void Uart_SendByte(uint8_t DataByte)      
    304          {   
    305              //===单线通讯时，发送数据时避免影响接收管脚，故发送数据时先禁止接收及其中断
    306              //UART1_CR2 &= ~0x04;
    307              //UART1_CR2 &= ~0x20; 
    308              while(!UART1_SR_TXE);
    309              UART1_DR = DataByte;    
    310              nop(); nop(); nop();
    311              while(!(UART1_SR &0x40)); 
    312              //========数据发送完毕，使能接收及其中断
    313              UART1_SR &= ~0x48;
    314              //UART1_CR2 |= 0x24; 
    315          }  
