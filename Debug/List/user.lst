   1449          uint8_t CRC8_Caculate(uint8_t *ptr,uint8_t len)
   1450          {
   1451            uint8_t i;
   1452          	
   1453            uint8_t crc = 0;
   1454          
   1455            uint8_t key = 0x07; //_Poly_07
   1456            while((len--) != 0)
   1457          	
   1458            {
   1459          		
   1460              for(i = 0x80; i != 0; i /= 2)
   1461          		
   1462              {
   1463          			
   1464                if((crc & 0x80) != 0)
   1465          			
   1466                {
   1467          				
   1468                  crc *= 2;
   1469          				
   1470                  crc ^= key;
   1471          			
   1472                }
   1473          			
   1474                else
   1475          				
   1476                {
   1477                  crc *= 2;
   1478          
   1479                }
   1480          			
   1481                if((*ptr & i) != 0)
   1482          				
   1483                {
   1484                  crc ^= key;
   1485                }
   1486          		
   1487              }
   1488          		
   1489              ptr++;
   1490          	
   1491            }
   1492            return (crc);
   1493          
   1494          } 
