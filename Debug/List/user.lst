   1516          /*
   1517          In a single-byte read transaction, the CRC is calculated after the second start and uses the slave address and data byte.
   1518          */
   1519          void I2C_Read(uint8_t addr,uint8_t *data)//slave device address 0x18
   1520          {      
   1521            uint16_t OverTimeDelay = OverTimeDelay_SET;
   1522            uint8_t CRC_ReData,Retry_Cnt = 5; 
   1523            Buf[0] = SLAVE_ADDR; 
   1524            while(Retry_Cnt > 0)
   1525            {
   1526              //以下见stm8s中文数据手册P251（图96主设备发送模式发送序列图）  
   1527              disableInterrupts(); 
   1528              I2C->CR2 |= 0x04; //ack使能 
   1529              //=============================发送起始位 I2C_Start() 
   1530              while(I2C->SR3 & 0x02){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;I2C_COM_ERROR_Flag = 1;break;}}    // 等待总线空闲   检测i2c-SR3 busy位        
   1531              I2C->CR2 |= 0x01;        // 产生起始位     CR2 start位  
   1532              
   1533              //=============================发送器件地址 I2C_SendDAdr(0xD0)
   1534              //EV5：SB=1，读SR1 然后将地址写入DR寄存器将清除该标志。  
   1535              while(!(I2C->SR1 & 0x01));  //等待START发送完 E5  
   1536              I2C->DR = (SLAVE_ADDR << 1) + 0x00;  //发送 器件地址(最后一位是0,表示发送) 
   1537              
   1538              while(!(I2C->SR1 & 0x02 == 0x02)){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;break;}}// I2C_COM_ERROR_Flag = 1; //等特7位器件地址发送完并且收到ack,ADDR置1    
   1539              //while(!(I2C->SR1 & 0x02));  //等特7位器件地址发送完并且收到ack,ADDR置1    
   1540              //EV6:ADDR 在软件读取SR1后，对SR3寄存器读操作 将清除该位 
   1541              I2C->SR1; //见P251 读SR1 (实验证明可以不要)  
   1542              I2C->SR3; //然后读SR3 清  ADDR 
   1543              
   1544              if(I2C_COM_ERROR_Flag == 0)
   1545              {
   1546                //=============================DATA 发送寄存器地址 I2C_SendDat()  
   1547                I2C->DR = (u8)(addr);      
   1548                //EV8_2 TxE=1 ，BTF=1，产生停止条件时由硬件清除。   
   1549                while(!(I2C->SR1 & 0x84));  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1550                //在发送地址和清除ADDR 之后，I2C接口进入主设备接收模式。以下见stm8s中文数据手册P252（图97主设备接收模式接收序列图）  
   1551                
   1552                //=============================I2C_Start()        
