   1042            /*The ADC transfer function is a linear equation defined as follows:
   1043              V(cell) = GAIN x ADC(cell) + OFFSET
   1044              If the DEVICE_XREADY is set, the voltage register values from the device should not be used. If
   1045            */ 
   1046          void Afe_Volt_Val_Get(void)
   1047          { 
   1048            uint8_t i = 0;
   1049            uint16_t volt_sum = 0;
   1050            uint16_t volt_max = 0;
   1051            uint16_t volt_min = 0xFFFF;
   1052            //====================================250mS
   1053            if(Cell_Volt_Sample_Cnt >= 26)
   1054            {
   1055              Cell_Volt_Sample_Cnt = 0;
   1056              for(i =0;i< 20;i += 2)
   1057              {  
   1058                Adc_value[i/2] = Afe_Get_Adc((VC1_HI_ADDR + i)) & 0x3FFF; //VC1_HI_ADDR =0x0C
   1059                Cell_Volt[i/2] = (uint16_t)((uint32_t)ADCGain_Val *Adc_value[i/2]/1000) + ADCOffset_Val;
   1060                volt_sum += Cell_Volt[i/2];
   1061                if(Cell_Volt[i/2] < volt_min)
   1062                {
   1063                   volt_min = Cell_Volt[i/2];
   1064                }
   1065                if(Cell_Volt[i/2] > volt_max)
   1066                {
   1067                   volt_max = Cell_Volt[i/2];
   1068                }
   1069              } 
   1070              Cell_Volt_Tol = volt_sum; 
   1071              Cell_Volt_Avg = Cell_Volt_Tol/10; 
   1072              Cell_Volt_Max = volt_max; 
   1073              Cell_Volt_Min = volt_min; 
   1074                
   1075            }
   1076            //====================================250mS
   1077            /*  Once converted to digital form, each cell voltage is added up and the summation result stored in the BAT
   1078                registers. This 16-bit value has a nominal LSB of 1.532 mV. */
   1079            //Pack_Volt = (uint16_t)((uint32_t)4 *ADCGain_Val *Afe_Get_Adc(BAT_HI_ADDR)/1000+(10 *ADCOffset_Val));
   1080            //Pack_Volt = (uint16_t)((uint32_t)1532 *Afe_Get_Adc(BAT_HI_ADDR)/1000);
   1081            
   1082            //====================================250mS
   1083            //Positive values are charge current; negative values are discharge current.
   1084            //an average over the 250-ms integration period.
   1085            /*Why does the CC value read not change when I set the CC_ONESHOT bit?
   1086                  The CC_READY bit must be cleared before the CC_ONESHOT bit is set. Note also that the CC_EN bit
   1087                  should be cleared or a CC conversion may already be in progress.
   1088              The full scale range of the CC is ± 270 mV, with a max recommended input range of ± 200 mV, thus yielding an LSB of approximately 8.44 μV.
   1089              The following equation shows how to convert the 16-bit CC reading into an analog voltage if no boardlevel calibration is performed:
   1090              CC Reading (in μV) = [16-bit 2’s Complement Value] × (8.44 μV/LSB) 
   1091            */
   1092            if( SYS_STAT.Bit.CC_READY) //CC_Volt_Sample_Cnt >= 23 &&
   1093            {
   1094              CC_Volt_Sample_Cnt = 0;
   1095              SYS_STAT.Bit.CC_READY = 0;
   1096              CC_AD = Afe_Get_Adc(CC_HI_ADDR); 
   1097              //CC_Val = (int32_t)CC_AD * 820/100; //mA (int32_t)
   1098              CC_Val = (int32_t)820 * CC_AD /100; //mA (int32_t)
   1099              //SYS_STAT_Last |= 0x80;
   1100              //I2C_Write(SYS_STAT_ADDR,SYS_STAT_Last); 
   1101              //SYS_STAT_Last &= ~0x80;
   1102              Afe_CC_1Shot_Set();
   1103              if(CC_Val < -32000)
   1104              {
   1105                CC_Val = -32000;
   1106              }
   1107              else if(CC_Val > 32000)
   1108              {
   1109                CC_Val = 32000;
   1110              }
   1111              Current_Val = CC_Val;
   1112              if(CC_Val < 0)
   1113              {
   1114                Current_Val = -CC_Val;
   1115              }
   1116            }
   1117            //====================================2S 
   1118            if(Temp_Volt_Sample_Cnt >= 210)
   1119            {
   1120              //V_TS2_Val = (uint16_t)((uint32_t)(382 * ((uint16_t)adcval <<8 | tmpval))/1000);//mV
   1121              V_TS2_Val = Afe_Get_Adc(TS2_HI_ADDR) &0x3FFF ;
   1122              V_TS2_Val = (uint16_t)(((uint32_t)382 * V_TS2_Val /1000));  //mV
   1123              V_TS2_Val = (uint16_t)(((uint32_t)1024 * V_TS2_Val /3300));  //mV
   1124              //R_TS2_Val = (uint16_t)((uint32_t)10000 * V_TS2_Val)/(3300 - V_TS2_Val); 
   1125            }
