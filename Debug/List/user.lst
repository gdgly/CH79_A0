   1396          uint8_t CRC8_Caculate(uint8_t *ptr,uint8_t len)
   1397          {
   1398            uint8_t i;
   1399          	
   1400            uint8_t crc = 0;
   1401          
   1402            uint8_t key = 0x07; //_Poly_07
   1403            while((len--) != 0)
   1404          	
   1405            {
   1406          		
   1407              for(i = 0x80; i != 0; i /= 2)
   1408          		
   1409              {
   1410          			
   1411                if((crc & 0x80) != 0)
   1412          			
   1413                {
   1414          				
   1415                  crc *= 2;
   1416          				
   1417                  crc ^= key;
   1418          			
   1419                }
   1420          			
   1421                else
   1422          				
   1423                {
   1424                  crc *= 2;
   1425          
   1426                }
   1427          			
   1428                if((*ptr & i) != 0)
   1429          				
   1430                {
   1431                  crc ^= key;
   1432                }
   1433          		
   1434              }
   1435          		
   1436              ptr++;
   1437          	
   1438            }
   1439            return (crc);
   1440          
   1441          } 
