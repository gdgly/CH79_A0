   1893          void SOC_Init(void)
   1894          {
   1895            uint8_t result = 0;
   1896            uint8_t Soc_Tmp = 0;
   1897            result = FLASH_ReadByte(ADJUST_ADDR);
   1898            
   1899            if(result == 0xAA)//判断校验地址中数据是否为0xAA, Y: EEPROM 中保存有SOC数据;  N: EEPROM 中保存有SOC数据
   1900            {
   1901              FLASH_Unlock(FLASH_MEMTYPE_DATA);       // 解锁EEPROM
   1902              FLASH_ProgramByte(ADJUST_ADDR,0x00);    // 清除校验地址中数据 
   1903              FLASH_Lock(FLASH_MEMTYPE_DATA);         // 加锁EEPROM
   1904              Soc_Tmp = FLASH_ReadByte(SOC_ADDR);     // 读取SOC数据
   1905               
   1906              SocReg.soc = Soc_Tmp; //SocReg.ah = SocCalc.curAh; // 计算SOC。 
   1907              SocCalc.curAh = ((uint32_t)SocReg.rated_cap * Soc_Tmp) / 100;
   1908              SocReg.ah = SocCalc.curAh;
   1909              SocCalc.soc_rt = SocReg.soc;
   1910              Soc_OCV_CorrectEn_Flag = 0;  // 上电禁止SOC的OCV校准  
   1911            }
   1912            else
   1913            {
   1914              Soc_OCV_CorrectEn_Flag = 1;  // 上电允许SOC的OCV校准
   1915            } 
   1916          }
   1917          void SOC_SavedtoEEPROM(void)
   1918          {   
   1919            FLASH_Unlock(FLASH_MEMTYPE_DATA);               // 解锁EEPROM
   1920            FLASH_ProgramByte(SOC_ADDR,SocCalc.soc_rt);     // 保存SOC到EEPROM 
   1921            FLASH_Lock(FLASH_MEMTYPE_DATA);                 // 加锁EEPROM
   1922            if(SocCalc.soc_rt == FLASH_ReadByte(SOC_ADDR))  // 读取数据 ?= 写入数据
   1923            {
   1924              FLASH_Unlock(FLASH_MEMTYPE_DATA);             // 解锁EEPROM
   1925              FLASH_ProgramByte(ADJUST_ADDR,0xAA);          // 写入保存SOC成功标志符
   1926              FLASH_Lock(FLASH_MEMTYPE_DATA);               // 加锁EEPROM
   1927            } 
   1928          }
