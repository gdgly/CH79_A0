    400          void Timer2Init(void)
    401          { 
    402              CLK_PCKENR1 |= 0x20;    // Fmaster 与外设TIM2连接
    403              TIM2_CR1 = 0x84;        // TIM2_ARR寄存器通过缓冲预装载，使能计数器
    404              TIM2_IER = 0x00;        // 禁止中断
    405              //TIM2_EGR = 0x01;        // 允许产生更新事件 
    406              TIM2_PSCR = 0x07;       // 计数器时钟=主时钟/8=16MHZ/128    8us// 相当于计数器周期为 
    407              TIM2_CNTRH = 0;         //     
    408              TIM2_CNTRL = 0;         //     
    409              TIM2_ARRH = 0x04;       //     10ms
    410              TIM2_ARRL = 0xE2;       //   
    411              TIM2_IER |= 0x01;       // 使能更新中断，禁止触发中断  
    412              TIM2_CR1 |= 0x01;        // TIM2_ARR寄存器通过缓冲预装载，使能计数器
    413          } 
    414          //=======================================================================
    415          //==================================================================
    416          //==================================================================
    417          //--Fmaster = CK_PSC =16MHz, CK_PSC prescaler(128) to CK_CNT(8us)   TIM4_SR1.UIF更新中断标志符
    418          void Timer4_Init_us(void)
    419          {
    420              CLK_PCKENR1 |= 0x10;    //Fmaster 与外设TIM4连接
    421              TIM4_IER = 0x00;        // 禁止中断
    422              TIM4_EGR = 0x01;        // 允许产生更新事件 
    423              //TIM4_PSCR = 0x07;       // 计数器时钟=主时钟/128=16MHZ/128  // 相当于计数器周期为8uS
    424              TIM4_PSCR = 0x00;       // 计数器时钟=主时钟/128=16MHZ/1  // 相当于计数器周期为0.0625uS
    425              TIM4_CR1 = 0x15;        // TIM4_ARR寄存器通过缓冲预装载，使能计数器
    426              TIM4_CNTR = 0;          //     
    427              TIM4_ARR = 16;           // 0.0625*16 = 1us  // 6*8us = 200us  
    428              TIM4_IER |= 0x01;       //更新中断使能，禁止触发中断 
    429              TIM4_CR1 |= 0x01; 
    430          } 
    431          void Timer4_Init_ms(void)
    432          {
    433              CLK_PCKENR1 |= 0x10;    //Fmaster 与外设TIM4连接
    434              TIM4_IER = 0x00;        // 禁止中断
    435              TIM4_EGR = 0x01;        // 允许产生更新事件 
    436              TIM4_PSCR = 0x07;       // 计数器时钟=主时钟/128=16MHZ/128  // 相当于计数器周期为8uS 
    437              TIM4_CR1 = 0x15;        // TIM4_ARR寄存器通过缓冲预装载，使能计数器
    438              TIM4_CNTR = 0;          //     
    439              TIM4_ARR = 125;         // 125*8us = 1ms  
    440              TIM4_IER |= 0x01;       //更新中断使能，禁止触发中断 
    441              TIM4_CR1 |= 0x01; 
    442          } 
    443          void Timer4_Disable(void)
    444          {
    445              TIM4_CR1 &= ~0x01;
    446              TIM4_IER &= ~0x01;
    447              CLK_PCKENR1 &= ~0x10;    //Fmaster 与外设TIM4断开连接 
    448          } 
    449          void Delay_us(uint16_t Delay_time)
    450          {
    451            Delay_time_t = Delay_time;
    452            Timer4_Init_us();
    453            while(Delay_time_t > 0);  
    454            Timer4_Disable();
    455          }
    456          void Delay_ms(uint16_t Delay_time)
    457          {
    458            Delay_time_t = Delay_time;
    459            Timer4_Init_ms();
    460            while(Delay_time_t > 0)
    461            {
    462              if(Delay_time_t >= 300)
    463              {  
    464                ;//ClrWdt(); 
    465              } 
    466            }
    467            Timer4_Disable();
    468          }
