    361          void Uart_SendData(unsigned int tx_data, int base) 
    362          {
    363            unsigned char buf[20] = {0};
    364          
    365            itoa((char *)buf, tx_data, base);
    366            
    367            if (base == 16)
    368            {
    369              Uart_SendStr((unsigned char *)"0x");
    370            }
    371            
    372            Uart_SendStr(buf);
    373          }
