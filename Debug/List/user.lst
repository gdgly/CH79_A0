   1872              while(!(I2C->SR1 & 0x84));  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1873              //EV8_2：TxE=1，BTF=1，产生停止条件时由硬件清除。  
   1874              
   1875              //============================= 发送数据  
   1876              I2C->DR = CRC_data;    
   1877              while(!(I2C->SR1 & 0x84));  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1878              //EV8_2：TxE=1，BTF=1，产生停止条件时由硬件清除。  
   1879              
   1880              I2C->DR;  //清除I2C_SR2的BTF位
   1881              I2C->CR2 |= 0x02;  //停止位产生stop  //*注意： 发送停止位时，必须先清除I2C_SR2的BTF位  
   1882              
   1883              OverTimeDelay =100; while((OverTimeDelay --) >0);
   1884              if(I2C_COM_ERROR_Flag == 1)
   1885              {
   1886                return;
   1887              }
   1888              I2C_COM_ERROR_Flag = 0;
   1889              Write_Retry_Cnt -= 1;
   1890            }  
   1891          }
