   1445          /*
   1446          In a single-byte read transaction, the CRC is calculated after the second start and uses the slave address and data byte.
   1447          */
   1448          void I2C_Read(uint8_t addr,uint8_t *data)//slave device address 0x18
   1449          {      
   1450            uint16_t OverTimeDelay = OverTimeDelay_SET;
   1451            uint8_t CRC_ReData,Retry_Cnt = 5; 
   1452            Buf[0] = SLAVE_ADDR; 
   1453            while(Retry_Cnt > 0)
   1454            {
   1455              //以下见stm8s中文数据手册P251（图96主设备发送模式发送序列图）  
   1456              disableInterrupts(); 
   1457              I2C->CR2 |= 0x04; //ack使能 
   1458              //=============================发送起始位 I2C_Start() 
   1459              while(I2C->SR3 & 0x02){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;I2C_COM_ERROR_Flag = 1;break;}}    // 等待总线空闲   检测i2c-SR3 busy位        
   1460              I2C->CR2 |= 0x01;        // 产生起始位     CR2 start位  
   1461              
   1462              //=============================发送器件地址 I2C_SendDAdr(0xD0)
   1463              //EV5：SB=1，读SR1 然后将地址写入DR寄存器将清除该标志。  
   1464              while(!(I2C->SR1 & 0x01));  //等待START发送完 E5  
   1465              I2C->DR = (SLAVE_ADDR << 1) + 0x00;  //发送 器件地址(最后一位是0,表示发送) 
   1466              
   1467              while(!(I2C->SR1 & 0x02 == 0x02)){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;break;}}// I2C_COM_ERROR_Flag = 1; //等特7位器件地址发送完并且收到ack,ADDR置1    
   1468              //while(!(I2C->SR1 & 0x02));  //等特7位器件地址发送完并且收到ack,ADDR置1    
   1469              //EV6:ADDR 在软件读取SR1后，对SR3寄存器读操作 将清除该位 
   1470              I2C->SR1; //见P251 读SR1 (实验证明可以不要)  
   1471              I2C->SR3; //然后读SR3 清  ADDR 
   1472              
   1473              if(I2C_COM_ERROR_Flag == 0)
   1474              {
   1475                //=============================DATA 发送寄存器地址 I2C_SendDat()  
   1476                I2C->DR = (u8)(addr);      
   1477                //EV8_2 TxE=1 ，BTF=1，产生停止条件时由硬件清除。   
   1478                while(!(I2C->SR1 & 0x84));  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1479                //在发送地址和清除ADDR 之后，I2C接口进入主设备接收模式。以下见stm8s中文数据手册P252（图97主设备接收模式接收序列图）  
   1480                
   1481                //=============================I2C_Start()        
   1482                I2C->CR2 |= 0x01;  //产生重复起始位   
   1483                //EV5：SB=1，读SR1 然后将地址写入DR寄存器将清除该标志。  
   1484                while(!(I2C->SR1 & 0x01));  //等待START发送完     
   1485                //ADDRESS (接收)   
   1486                I2C->DR = (SLAVE_ADDR <<1) + 0x01;  //发送MLX90615器件地址(最后一位是1,表示接收)，发送完后自动发送ack(提前是CR2 ack位使能)      
   1487                //EV6:ADDR 在软件读取SR1后，对SR3寄存器读操作 将清除改位   
   1488                
   1489                while(!(I2C->SR1 & 0x02 == 0x02)){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;break;}} // I2C_COM_ERROR_Flag = 1;//等特7位器件地址发送完并且收到ack,ADDR置1    
   1490                //while(!(I2C->SR1 & 0x02));  //等特7位器件地址发送完并且收到ack,ADDR置1  
   1491                I2C->SR1; //见P251 读SR1 (实验证明可以不要)  
   1492                I2C->SR3; //然后读SR3 清  ADDR   
   1493                   Buf[0] = 0x31;  Buf[1] = 0;     Buf[2] = 0;           
   1494                //测试EV7 RxNE=1（收到一个字节后RxNE置1） ，判断DR寄存器有数据    
   1495                while(!(I2C->SR1 & 0x40));                                        
   1496                Buf[1] = I2C->DR;//在接收模式下，收到完整字节后,自动发送ack(提前是CR2 ack位使能，不需要专门CR2 ack位置1)                                               
   1497                 
   1498               
   1499                
   1500                //测试EV7 RxNE=1（收到一个字节后RxNE置1） ，判断DR寄存器有数据    
   1501                while(!(I2C->SR1 & 0x40));                                        
   1502                Buf[2] = I2C->DR;//在接收模式下，收到完整字节后,自动发送ack(提前是CR2 ack位使能，不需要专门CR2 ack位置1)  
   1503                  //EV7_1 ：RxNE=1 ，读DR寄存器清除该标志。设置ACK=0和STOP 请求。(在接收最后一个字节前)     
   1504                I2C->SR3;
   1505                I2C->DR; 
   1506                I2C->CR2 &= ~0x04; //ack使能  
   1507                I2C->CR2 |= 0x02;  //停止位产生stop      
   1508                 
   1509                enableInterrupts(); 
   1510                //while((I2C->CR2 & I2C_CR2_STOP));
   1511                CRC_ReData = CRC8_Caculate(Buf,2);
   1512                /*Uart_SendByte(0x11);
   1513                Uart_SendByte(Buf[0]);
   1514                Uart_SendByte(Buf[1]);
   1515                Uart_SendByte(Buf[2]);*/
   1516                if(CRC_ReData == Buf[2])
   1517                {
   1518                  *data = Buf[1];
   1519                  OverTimeDelay = 200; while((OverTimeDelay --) >0);
   1520                  return;
   1521                }
   1522              }
   1523              I2C_COM_ERROR_Flag = 0;
   1524              Retry_Cnt -= 1;
   1525            }
   1526            enableInterrupts(); 
   1527            //Uart_SendByte(0xDD);
   1528            //Uart_SendByte(0xDD);
   1529          }
