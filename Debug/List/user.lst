    971            /*The ADC transfer function is a linear equation defined as follows:
    972              V(cell) = GAIN x ADC(cell) + OFFSET
    973              If the DEVICE_XREADY is set, the voltage register values from the device should not be used. If
    974            */ 
    975          void Afe_Volt_Val_Get(void)
    976          { 
    977            uint8_t i = 0;
    978            uint16_t volt_sum = 0;
    979            uint16_t volt_max = 0;
    980            uint16_t volt_min = 0xFFFF;
    981            //====================================250mS
    982            if(Cell_Volt_Sample_Cnt >= 26)
    983            {
    984              Cell_Volt_Sample_Cnt = 0;
    985              for(i =0;i< 20;i += 2)
    986              {  
    987                Adc_value[i/2] = Afe_Get_Adc((VC1_HI_ADDR + i)) & 0x3FFF; //VC1_HI_ADDR =0x0C
    988                Cell_Volt[i/2] = (uint16_t)((uint32_t)ADCGain_Val *Adc_value[i/2]/1000) + ADCOffset_Val;
    989                volt_sum += Cell_Volt[i/2];
    990                if(Cell_Volt[i/2] < volt_min)
    991                {
    992                   volt_min = Cell_Volt[i/2];
    993                }
    994                if(Cell_Volt[i/2] > volt_max)
    995                {
    996                   volt_max = Cell_Volt[i/2];
    997                }
    998              } 
    999              Cell_Volt_Tol = volt_sum; 
   1000              Cell_Volt_Avg = Cell_Volt_Tol/10; 
   1001              Cell_Volt_Max = volt_max; 
   1002              Cell_Volt_Min = volt_min; 
   1003                
   1004            }
   1005            //====================================250mS
   1006            /*  Once converted to digital form, each cell voltage is added up and the summation result stored in the BAT
   1007                registers. This 16-bit value has a nominal LSB of 1.532 mV. */
   1008            //Pack_Volt = (uint16_t)((uint32_t)4 *ADCGain_Val *Afe_Get_Adc(BAT_HI_ADDR)/1000+(10 *ADCOffset_Val));
   1009            //Pack_Volt = (uint16_t)((uint32_t)1532 *Afe_Get_Adc(BAT_HI_ADDR)/1000);
   1010            
   1011            //====================================250mS
   1012            //Positive values are charge current; negative values are discharge current.
   1013            //an average over the 250-ms integration period.
   1014            /*Why does the CC value read not change when I set the CC_ONESHOT bit?
   1015                  The CC_READY bit must be cleared before the CC_ONESHOT bit is set. Note also that the CC_EN bit
   1016                  should be cleared or a CC conversion may already be in progress.
   1017              The full scale range of the CC is ± 270 mV, with a max recommended input range of ± 200 mV, thus yielding an LSB of approximately 8.44 μV.
   1018              The following equation shows how to convert the 16-bit CC reading into an analog voltage if no boardlevel calibration is performed:
   1019              CC Reading (in μV) = [16-bit 2’s Complement Value] × (8.44 μV/LSB) 
   1020            */
   1021            if((CC_Volt_Sample_Cnt >= 26) && SYS_STAT.Bit.CC_READY) //(0)//
   1022            {
   1023              CC_Volt_Sample_Cnt = 0;
   1024              SYS_STAT.Bit.CC_READY = 0;
   1025              CC_AD = Afe_Get_Adc(CC_HI_ADDR);  
   1026              //CC_Val = (int32_t)820 * CC_AD /100; //mA (int32_t)
   1027              CC_Val = (int32_t)820 * CC_AD /500; //mA (int32_t)
   1028               
   1029              Afe_CC_1Shot_Set();
   1030              if(CC_Val < -320000)
   1031              {
   1032                CC_Val = -320000;
   1033              }
   1034              else if(CC_Val > 320000)
   1035              {
   1036                CC_Val = 320000;
   1037              }
   1038              Current_Val = CC_Val;
   1039              if(CC_Val < 0)
   1040              {
   1041                Current_Val = -CC_Val;
   1042              }
   1043            }
   1044            //====================================2S 
   1045            if(Temp_Volt_Sample_Cnt >= 210)
   1046            {
   1047              //V_TS2_Val = (uint16_t)((uint32_t)(382 * ((uint16_t)adcval <<8 | tmpval))/1000);//mV
   1048              V_TS2_Val = Afe_Get_Adc(TS2_HI_ADDR) &0x3FFF ;
   1049              V_TS2_Val = (uint16_t)(((uint32_t)382 * V_TS2_Val /1000));  //mV
   1050              V_TS2_Val = (uint16_t)(((uint32_t)4096 * V_TS2_Val /3300));  //mV
   1051              //R_TS2_Val = (uint16_t)((uint32_t)10000 * V_TS2_Val)/(3300 - V_TS2_Val); 
   1052            }
   1053            //=====================================================================================
   1054            
   1055          }
