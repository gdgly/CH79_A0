   1555          uint8_t CRC8_Caculate(uint8_t *ptr,uint8_t len)
   1556          {
   1557            uint8_t i;
   1558          	
   1559            uint8_t crc = 0;
   1560          
   1561            uint8_t key = 0x07; //_Poly_07
   1562            while((len--) != 0)
   1563          	
   1564            {
   1565          		
   1566              for(i = 0x80; i != 0; i /= 2)
   1567          		
   1568              {
   1569          			
   1570                if((crc & 0x80) != 0)
   1571          			
   1572                {
   1573          				
   1574                  crc *= 2;
   1575          				
   1576                  crc ^= key;
   1577          			
   1578                }
   1579          			
   1580                else
   1581          				
   1582                {
   1583                  crc *= 2;
   1584          
   1585                }
   1586          			
   1587                if((*ptr & i) != 0)
   1588          				
   1589                {
   1590                  crc ^= key;
   1591                }
   1592          		
   1593              }
   1594          		
   1595              ptr++;
   1596          	
   1597            }
   1598            return (crc);
