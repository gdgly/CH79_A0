   1871              while(!(I2C->SR1 & 0x84));  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1872              //EV8_2：TxE=1，BTF=1，产生停止条件时由硬件清除。  
   1873              
   1874              //============================= 发送数据  
   1875              I2C->DR = CRC_data;    
   1876              while(!(I2C->SR1 & 0x84));  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1877              //EV8_2：TxE=1，BTF=1，产生停止条件时由硬件清除。  
   1878              
   1879              I2C->DR;  //清除I2C_SR2的BTF位
   1880              I2C->CR2 |= 0x02;  //停止位产生stop  //*注意： 发送停止位时，必须先清除I2C_SR2的BTF位  
   1881              
   1882              OverTimeDelay =100; while((OverTimeDelay --) >0);
   1883              if(I2C_COM_ERROR_Flag == 1)
   1884              {
   1885                return;
   1886              }
   1887              I2C_COM_ERROR_Flag = 0;
   1888              Write_Retry_Cnt -= 1;
   1889            }  
   1890          }
