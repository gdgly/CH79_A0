   1485          uint8_t CRC8_Caculate(uint8_t *ptr,uint8_t len)
   1486          {
   1487            uint8_t i;
   1488          	
   1489            uint8_t crc = 0;
   1490          
   1491            uint8_t key = 0x07; //_Poly_07
   1492            while((len--) != 0)
   1493          	
   1494            {
   1495          		
   1496              for(i = 0x80; i != 0; i /= 2)
   1497          		
   1498              {
   1499          			
   1500                if((crc & 0x80) != 0)
   1501          			
   1502                {
   1503          				
   1504                  crc *= 2;
   1505          				
   1506                  crc ^= key;
   1507          			
   1508                }
   1509          			
   1510                else
   1511          				
   1512                {
   1513                  crc *= 2;
   1514          
   1515                }
   1516          			
   1517                if((*ptr & i) != 0)
   1518          				
   1519                {
   1520                  crc ^= key;
   1521                }
   1522          		
   1523              }
   1524          		
   1525              ptr++;
   1526          	
   1527            }
   1528            return (crc);
   1529          
