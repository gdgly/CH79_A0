   1065                    {   
   1066                      CellBalance_Cur_Selct |= (0x0001 << i); 
   1067                    }
   1068                    else
   1069                    {
   1070                      CellBalance_Cur_Selct &= ~(0x0001 << i);
   1071                    }
   1072                  }  
   1073                  CellBalance_Selct = 0x0000;
   1074                  //===================================
   1075                  for(i =0; i <10; i++ )
   1076                  {
   1077                    if((CellBalance_Cur_Selct &(0x0001 <<i)))
   1078                    {
   1079                      CellBalance_Selct |=(0x0001 <<i);
   1080                      if(4 != i)
   1081                      {
   1082                        i +=1; 
   1083                      }
   1084                    }  
   1085                  } 
   1086                  Afe_CellBalance_Enable(CellBalance_Selct);
   1087                } 
   1088                else if(CellBal_Cntrl_Lock ==1)
   1089                { 
   1090                  Afe_CellBalance_Disable();
   1091                } 
   1092              }
   1093            }
   1094            else if(CellBal_Cntrl_Lock ==1)
   1095            {
   1096                Afe_CellBalance_Disable();
   1097            }
   1098          }
