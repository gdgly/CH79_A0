    951              {  
    952                Adc_value[i/2] = Afe_Get_Adc((VC1_HI_ADDR + i)) & 0x3FFF; //VC1_HI_ADDR =0x0C
    953                Cell_Volt[i/2] = (uint16_t)((uint32_t)ADCGain_Val *Adc_value[i/2]/1000) + ADCOffset_Val;
    954                volt_sum += Cell_Volt[i/2];
    955                if(Cell_Volt[i/2] < volt_min)
    956                {
    957                   volt_min = Cell_Volt[i/2];
    958                }
    959                if(Cell_Volt[i/2] > volt_max)
    960                {
    961                   volt_max = Cell_Volt[i/2];
    962                }
    963              } 
    964              Cell_Volt_Tol = volt_sum; 
    965              Cell_Volt_Avg = Cell_Volt_Tol/10; 
    966              Cell_Volt_Max = volt_max; 
    967              Cell_Volt_Min = volt_min; 
    968                
    969            }
    970            //====================================250mS
    971            /*  Once converted to digital form, each cell voltage is added up and the summation result stored in the BAT
    972                registers. This 16-bit value has a nominal LSB of 1.532 mV. */
    973            //Pack_Volt = (uint16_t)((uint32_t)4 *ADCGain_Val *Afe_Get_Adc(BAT_HI_ADDR)/1000+(10 *ADCOffset_Val));
    974            //Pack_Volt = (uint16_t)((uint32_t)1532 *Afe_Get_Adc(BAT_HI_ADDR)/1000);
    975            
    976            //====================================250mS
    977            //Positive values are charge current; negative values are discharge current.
    978            //an average over the 250-ms integration period.
    979            /*Why does the CC value read not change when I set the CC_ONESHOT bit?
    980                  The CC_READY bit must be cleared before the CC_ONESHOT bit is set. Note also that the CC_EN bit
    981                  should be cleared or a CC conversion may already be in progress.
    982              The full scale range of the CC is ± 270 mV, with a max recommended input range of ± 200 mV, thus yielding an LSB of approximately 8.44 μV.
    983              The following equation shows how to convert the 16-bit CC reading into an analog voltage if no boardlevel calibration is performed:
    984              CC Reading (in μV) = [16-bit 2’s Complement Value] × (8.44 μV/LSB) 
    985            */
    986            if( SYS_STAT.Bit.CC_READY) //CC_Volt_Sample_Cnt >= 23 &&
    987            {
    988              CC_Volt_Sample_Cnt = 0;
    989              SYS_STAT.Bit.CC_READY = 0;
    990              CC_AD = Afe_Get_Adc(CC_HI_ADDR);  
    991              //CC_Val = (int32_t)820 * CC_AD /100; //mA (int32_t)
    992              CC_Val = (int32_t)820 * CC_AD /500; //mA (int32_t)
    993               
    994              Afe_CC_1Shot_Set();
    995              if(CC_Val < -320000)
    996              {
    997                CC_Val = -320000;
    998              }
    999              else if(CC_Val > 320000)
   1000              {
   1001                CC_Val = 320000;
   1002              }
   1003              Current_Val = CC_Val;
   1004              if(CC_Val < 0)
   1005              {
   1006                Current_Val = -CC_Val;
   1007              }
   1008            }
   1009            //====================================2S 
   1010            if(Temp_Volt_Sample_Cnt >= 210)
   1011            {
   1012              //V_TS2_Val = (uint16_t)((uint32_t)(382 * ((uint16_t)adcval <<8 | tmpval))/1000);//mV
   1013              V_TS2_Val = Afe_Get_Adc(TS2_HI_ADDR) &0x3FFF ;
   1014              V_TS2_Val = (uint16_t)(((uint32_t)382 * V_TS2_Val /1000));  //mV
   1015              V_TS2_Val = (uint16_t)(((uint32_t)1024 * V_TS2_Val /3300));  //mV
   1016              //R_TS2_Val = (uint16_t)((uint32_t)10000 * V_TS2_Val)/(3300 - V_TS2_Val); 
   1017            }
   1018            //=====================================================================================
   1019            
   1020          }
