    693          void ModeCheck_Backup(void)
    694          {       
    695            //工作模式的检测是依据外部Triger端的电压大小来判定
    696            Check_Val = ADConverse(3); 
    697            if((Check_Val >= 130 && Check_Val < 160) || CC_Val >= 10)
    698            {  
    699              if(ChgExchangeMode_Cnt >= 30)
    700              { 
    701                ChgExchangeMode_Cnt  = 100;
    702                DisExchangeMode_Cnt  = 0;
    703                IdleExchangeMode_Cnt = 0;
    704                WorkMode = CHARGE_MODE;
