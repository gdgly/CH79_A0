    724          void ModeCheck(void)
    725          {       
    726            static uint8_t ModeChange_Lock = 0;
    727            uint32_t OV_tmp = 1;
    728            uint32_t UV_tmp = 1;
    729            uint8_t Trip_tmp = 1;
    730            uint16_t Check_Val_tmp = 0;
    731            //工作模式的检测是依据外部Triger端的电压大小来判定 
    732            // 问题3：（充放电及空载识别问题）欠压状态下，插上充电器时，识别不了充电器;  
    733                    //解决方案：检测无Triger信号时，为空载模式;
    734                                //Triger信号范围在[220,630]时【[350,580]】，或检测到有负载信号、或放电电流 >= 10mA 时，为放电模式，否则为充电模式.
    735            
    736          //========================================================================== 
    737            
    738            //OV_TRIP_Last = (uint8_t)(((uint32_t)1000 * (OV_val - ADCOffset_Val)/ADCGain_Val) >> 4);  
    739            //UV_TRIP_Last = (uint8_t)(((uint32_t)1000 * (UV_val - ADCOffset_Val)/ADCGain_Val) >> 4); 
    740            Check_Val = ADConverse(3); // 充电时Check_Val_MAX == 150
    741             
    742            if(Check_Val < 10)// 无信号
    743            { 
    744              if(IdleExchangeMode_Cnt >= 100)
    745              { 
    746                IdleExchangeMode_Cnt = 100;
    747                ChgExchangeMode_Cnt = 0;
    748                DisExchangeMode_Cnt = 0;
    749                WorkMode = IDLE_MODE;
    750                if(0 != ModeChange_Lock)
    751                { 
    752                  I2C_Read(OV_TRIP_ADDR,&Trip_tmp); 
    753                  OV_tmp = ADCOffset_Val + (((uint32_t)OV_TRIP_Last << 4) * ADCGain_Val)/1000;
    754                    
    755                  I2C_Read(UV_TRIP_ADDR,&Trip_tmp); 
    756                  UV_tmp = ADCOffset_Val + (((uint32_t)UV_TRIP_Last << 4) * ADCGain_Val)/1000;
    757                    
    758                  if((OV_THREHOLD_VAL_SET != OV_tmp) || (UV_THREHOLD_VAL_SET != UV_tmp ))
    759                  {
    760                    Afe_OV_UV_Threshold_Set(OV_THREHOLD_VAL_SET, UV_THREHOLD_VAL_SET); 
    761                  }
    762                  else
    763                  {
    764                    ModeChange_Lock = 0;
    765                  } 
    766                }
    767              }
    768            } 
    769            else
    770            {
    771              // dsg: Cell_Volt_Total == 31070   Check_Val == [385,394]   Check_Val_tmp == [31018,31743]  Max_Delta == 673
    772              // chg: Cell_Volt_Total == 31050   Check_Val == [204,208]   Check_Val_tmp == [16435,16757]  Max_Delta == [14615,14293]
    773              Check_Val_tmp = (uint16_t)((uint32_t)Check_Val *3300 * 25 /1024);//+-1000mV
    774              if( ((Check_Val_tmp < Cell_Volt_Tol) && (Cell_Volt_Tol - Check_Val_tmp <= 2000)) || ((Check_Val_tmp > Cell_Volt_Tol) && (Check_Val_tmp - Cell_Volt_Tol <= 2000)) || SYS_CTRL1.Bit.LOAD_PRESENT) 
    775              //else if((Check_Val >= 186 && Check_Val < 630) || SYS_CTRL1.Bit.LOAD_PRESENT)// || CC_Val < (-10) )
    776              {  
    777                if(DisExchangeMode_Cnt >= 30)
    778                { 
    779                  IdleExchangeMode_Cnt = 0;
    780                  ChgExchangeMode_Cnt = 0;
    781                  DisExchangeMode_Cnt = 100; 
    782                  WorkMode = DISCHARGE_MODE;
    783                  if(2 != ModeChange_Lock)
    784                  {
    785                    //ModeChange_Lock = 2; 
    786                    //Afe_OV_UV_Threshold_Set(4300, UV_THREHOLD_VAL_SET); // 问题1：放电状态下，电芯电压处于过充时，BQ会自动关闭充电MOS管;   解决方案：放电状态下，重新设置硬件过充电压为4.3V
    787                    I2C_Read(OV_TRIP_ADDR,&Trip_tmp); 
    788                    OV_tmp = ADCOffset_Val + (((uint32_t)OV_TRIP_Last << 4) * ADCGain_Val)/1000;
    789                      
    790                    I2C_Read(UV_TRIP_ADDR,&Trip_tmp); 
    791                    UV_tmp = ADCOffset_Val + (((uint32_t)UV_TRIP_Last << 4) * ADCGain_Val)/1000;
    792                      
    793                     //if((4250 >= OV_tmp) || (2950 >= UV_tmp ))//3000
    794                    if((4300 != OV_tmp) || (UV_THREHOLD_VAL_SET != UV_tmp ))
    795                    {
    796                      Afe_OV_UV_Threshold_Set(4300, UV_THREHOLD_VAL_SET); 
    797                    }
    798                    else
    799                    {
    800                      ModeChange_Lock = 2;
    801                    }
    802                  }
    803                }
    804              }  
    805              else// if((Check_Val >= 130 && Check_Val < 160) || CC_Val >= 10)
    806              {  
    807                if(ChgExchangeMode_Cnt >= 30)
    808                { 
    809                  ChgExchangeMode_Cnt  = 100;
    810                  DisExchangeMode_Cnt  = 0;
    811                  IdleExchangeMode_Cnt = 0;
    812                  WorkMode = CHARGE_MODE;
    813                  if(1 != ModeChange_Lock)
    814                  {
    815                    //ModeChange_Lock = 1; 
    816                    //Afe_OV_UV_Threshold_Set(OV_THREHOLD_VAL_SET, 0);// 问题2：充电状态下，电芯电压处于过放时，BQ会自动关闭放电MOS管;   解决方案：充电状态下，重新设置硬件过充电压为1.5V
    817                    I2C_Read(OV_TRIP_ADDR,&Trip_tmp); 
    818                    OV_tmp = ADCOffset_Val + (((uint32_t)OV_TRIP_Last << 4) * ADCGain_Val)/1000;
    819                      
    820                    I2C_Read(UV_TRIP_ADDR,&Trip_tmp); 
    821                    UV_tmp = ADCOffset_Val + (((uint32_t)UV_TRIP_Last << 4) * ADCGain_Val)/1000;
    822                      
    823                    //if((4200 < OV_tmp) || (50 >= UV_tmp ))
    824                    if((OV_THREHOLD_VAL_SET != OV_tmp) || (0 != UV_tmp ))
    825                    {
    826                      Afe_OV_UV_Threshold_Set(OV_THREHOLD_VAL_SET, 0); 
    827                    }
    828                    else
    829                    {
    830                      ModeChange_Lock = 1;
    831                    }
    832                  }
    833                }
    834              }  
    835            }
    836          }
