   1093                    if(Cell_Volt[i] > CELLBALANCE_BEGIN_VAL && ((Cell_Volt[i] - Cell_Volt_Min) >= CELLBALANCE_DELTA_VAL))
   1094                    {   
   1095                      CellBalance_Cur_Selct |= (0x0001 << i); 
   1096                    }
   1097                    else
   1098                    {
   1099                      CellBalance_Cur_Selct &= ~(0x0001 << i);
   1100                    }
   1101                  }  
   1102                  CellBalance_Selct = 0x0000;
   1103                  //===================================
   1104                  for(i =0; i <10; i++ )
   1105                  {
   1106                    if((CellBalance_Cur_Selct &(0x0001 <<i)))
   1107                    {
   1108                      CellBalance_Selct |=(0x0001 <<i);
   1109                      if(4 != i)
   1110                      {
   1111                        i +=1; 
   1112                      }
   1113                    }  
   1114                  } 
   1115                  Afe_CellBalance_Enable(CellBalance_Selct);
   1116                } 
   1117                else if(CellBal_Cntrl_Lock ==1)
   1118                { 
   1119                  Afe_CellBalance_Disable();
   1120                } 
   1121              }
   1122            }
   1123            else if(CellBal_Cntrl_Lock ==1)
   1124            {
   1125                Afe_CellBalance_Disable();
   1126            }
   1127          }
