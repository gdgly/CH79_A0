   1236          void LowPower_Entry_MCU_Set(void)
   1237          {   
   1238            static uint8_t LowPower_Entry_Exit_Cnt = 0;
   1239            if(WorkMode == IDLE_MODE && (LowPower_Entry_Delay_t >= 500 || LowPower_MCU_Entry_Flag == 1))
   1240            { 
   1241              //if(LowPower_MCU_Entry_Flag == 0)
   1242              {
   1243                 Afe_ADC_Disable(); Afe_Temp_Disable();  
   1244              }
   1245              CLK_PCKENR2 = CLK_PCKENR2_AWU;
   1246              AWU_Init(AWU_TIMEBASE_1S); 
   1247              CLK->ICKR |= CLK_ICKR_SWUAH;
   1248              FLASH->CR1 |= 0x04;	  
   1249              EXTI_SetExtIntSensitivity(EXTI_PORT_GPIOD, EXTI_SENSITIVITY_FALL_ONLY);//Signal_In下降沿触发
   1250              ClrWdt();  
   1251              LowPower_MCU_Entry_Flag = 1; 
