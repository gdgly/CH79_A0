    360              TIM2_CNTRH = 0;         //     
    361              TIM2_CNTRL = 0;         //     
    362              TIM2_ARRH = 0x04;       //     10ms
    363              TIM2_ARRL = 0xE2;       //   
    364              TIM2_IER |= 0x01;       // 使能更新中断，禁止触发中断  
    365              TIM2_CR1 |= 0x01;        // TIM2_ARR寄存器通过缓冲预装载，使能计数器
    366          } 
