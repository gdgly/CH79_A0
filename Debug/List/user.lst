   1254              Delay_ms(5); 
   1255              //======================================
   1256              if(LowPower_MCU_Entry_Flag == 1)
   1257              {
   1258                if((LowPower_Entry_Exit_Cnt ++) >= 100) 
   1259                {
   1260                  LowPower_MCU_Entry_Flag = 0; 
   1261                  LowPower_Entry_Exit_Cnt = 0; 
   1262                }
   1263              }
   1264              else
   1265              {
   1266                LowPower_Entry_Exit_Cnt = 0; 
   1267                CLK_PCKENR2 &= ~CLK_PCKENR2_AWU; 
   1268                CLK->ICKR &= ~CLK_ICKR_SWUAH; 
   1269                Afe_ADC_Enable(); Afe_Temp_Enable();Delay_ms(10); 
   1270              } 
   1271              //=============================================
   1272              LowPower_Entry_Delay_t = 0; 
   1273            }
