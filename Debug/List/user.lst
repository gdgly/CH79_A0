   1804          void SOC_Init(void)
   1805          {
   1806            uint8_t result = 0;
   1807            uint8_t Soc_Tmp = 0;
   1808            result = FLASH_ReadByte(ADJUST_ADDR);
   1809            
   1810            if(result == 0xAA)//判断校验地址中数据是否为0xAA, Y: EEPROM 中保存有SOC数据;  N: EEPROM 中保存有SOC数据
   1811            {
   1812              FLASH_Unlock(FLASH_MEMTYPE_DATA);       // 解锁EEPROM
   1813              FLASH_ProgramByte(ADJUST_ADDR,0x00);    // 清除校验地址中数据 
   1814              FLASH_Lock(FLASH_MEMTYPE_DATA);         // 加锁EEPROM
   1815              Soc_Tmp = FLASH_ReadByte(SOC_ADDR);     // 读取SOC数据
   1816               
   1817              SocReg.soc = Soc_Tmp; //SocReg.ah = SocCalc.curAh; // 计算SOC。 
   1818              SocCalc.curAh = ((uint32_t)SocReg.rated_cap * Soc_Tmp) / 100;
   1819              SocReg.ah = SocCalc.curAh;
   1820              SocCalc.soc_rt = SocReg.soc;
   1821              Soc_OCV_CorrectEn_Flag = 0;  // 上电允许SOC的OCV校准  
   1822            }
   1823            else
   1824            {
   1825              Soc_OCV_CorrectEn_Flag = 1;  // 上电允许SOC的OCV校准
   1826            } 
   1827          }
   1828          void SOC_SavedtoEEPROM(void)
   1829          {   
   1830            FLASH_Unlock(FLASH_MEMTYPE_DATA);               // 解锁EEPROM
   1831            FLASH_ProgramByte(SOC_ADDR,SocCalc.soc_rt);     // 保存SOC到EEPROM 
   1832            FLASH_Lock(FLASH_MEMTYPE_DATA);                 // 加锁EEPROM
   1833            if(SocCalc.soc_rt == FLASH_ReadByte(SOC_ADDR))  // 读取数据 ?= 写入数据
   1834            {
   1835              FLASH_Unlock(FLASH_MEMTYPE_DATA);             // 解锁EEPROM
   1836              FLASH_ProgramByte(ADJUST_ADDR,0xAA);          // 写入保存SOC成功标志符
   1837              FLASH_Lock(FLASH_MEMTYPE_DATA);               // 加锁EEPROM
   1838            } 
   1839          }
