    898          //==========================================================================
    899            /*The ADC transfer function is a linear equation defined as follows:
    900              V(cell) = GAIN x ADC(cell) + OFFSET
    901              If the DEVICE_XREADY is set, the voltage register values from the device should not be used. If
    902            */ 
    903          void Afe_Volt_Val_Get(void)
    904          { 
    905            uint8_t i = 0;
    906            uint16_t volt_sum = 0;
    907            uint16_t volt_max = 0;
    908            uint16_t volt_min = 0xFFFF;
    909            //====================================250mS
    910            if(Cell_Volt_Sample_Cnt >= 26)
    911            {
    912              Cell_Volt_Sample_Cnt = 0;
    913              for(i =0;i< 20;i += 2)
    914              {  
    915                Adc_value[i/2] = Afe_Get_Adc((VC1_HI_ADDR + i)) & 0x3FFF; //VC1_HI_ADDR =0x0C
    916                Cell_Volt[i/2] = (uint16_t)((uint32_t)ADCGain_Val *Adc_value[i/2]/1000) + ADCOffset_Val;
    917                volt_sum += Cell_Volt[i/2];
    918                if(Cell_Volt[i/2] < volt_min)
    919                {
    920                   volt_min = Cell_Volt[i/2];
    921                }
    922                if(Cell_Volt[i/2] > volt_max)
    923                {
    924                   volt_max = Cell_Volt[i/2];
    925                }
    926              } 
    927              Cell_Volt_Tol = volt_sum; 
    928              Cell_Volt_Avg = Cell_Volt_Tol/10; 
    929              Cell_Volt_Max = volt_max; 
    930              Cell_Volt_Min = volt_min; 
    931                
    932            }
    933            //====================================250mS
    934            /*  Once converted to digital form, each cell voltage is added up and the summation result stored in the BAT
    935                registers. This 16-bit value has a nominal LSB of 1.532 mV. */
    936            //Pack_Volt = (uint16_t)((uint32_t)4 *ADCGain_Val *Afe_Get_Adc(BAT_HI_ADDR)/1000+(10 *ADCOffset_Val));
    937            //Pack_Volt = (uint16_t)((uint32_t)1532 *Afe_Get_Adc(BAT_HI_ADDR)/1000);
    938            
    939            //====================================250mS
    940            //Positive values are charge current; negative values are discharge current.
    941            //an average over the 250-ms integration period.
    942            /*Why does the CC value read not change when I set the CC_ONESHOT bit?
    943                  The CC_READY bit must be cleared before the CC_ONESHOT bit is set. Note also that the CC_EN bit
    944                  should be cleared or a CC conversion may already be in progress.
    945              The full scale range of the CC is ± 270 mV, with a max recommended input range of ± 200 mV, thus yielding an LSB of approximately 8.44 μV.
    946              The following equation shows how to convert the 16-bit CC reading into an analog voltage if no boardlevel calibration is performed:
    947              CC Reading (in μV) = [16-bit 2’s Complement Value] × (8.44 μV/LSB) 
    948            */
    949            if( SYS_STAT.Bit.CC_READY) //CC_Volt_Sample_Cnt >= 23 &&
    950            {
    951              CC_Volt_Sample_Cnt = 0;
    952              SYS_STAT.Bit.CC_READY = 0;
