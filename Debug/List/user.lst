   1467          uint8_t CRC8_Caculate(uint8_t *ptr,uint8_t len)
   1468          {
   1469            uint8_t i;
   1470          	
   1471            uint8_t crc = 0;
   1472          
   1473            uint8_t key = 0x07; //_Poly_07
   1474            while((len--) != 0)
   1475          	
   1476            {
   1477          		
   1478              for(i = 0x80; i != 0; i /= 2)
   1479          		
   1480              {
   1481          			
   1482                if((crc & 0x80) != 0)
   1483          			
   1484                {
   1485          				
   1486                  crc *= 2;
   1487          				
   1488                  crc ^= key;
   1489          			
   1490                }
   1491          			
   1492                else
   1493          				
   1494                {
   1495                  crc *= 2;
   1496          
   1497                }
   1498          			
   1499                if((*ptr & i) != 0)
   1500          				
   1501                {
   1502                  crc ^= key;
   1503                }
   1504          		
   1505              }
   1506          		
   1507              ptr++;
   1508          	
   1509            }
   1510            return (crc);
   1511          
   1512          } 
