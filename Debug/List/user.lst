   1086                    {   
   1087                      CellBalance_Cur_Selct |= (0x0001 << i); 
   1088                    }
   1089                    else
   1090                    {
   1091                      CellBalance_Cur_Selct &= ~(0x0001 << i);
   1092                    }
   1093                  }  
   1094                  CellBalance_Selct = 0x0000;
   1095                  //===================================
   1096                  for(i =0; i <10; i++ )
