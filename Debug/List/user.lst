    312          void Uart_SendData(unsigned int tx_data, int base) 
    313          {
    314            unsigned char buf[20] = {0};
    315          
    316            itoa((char *)buf, tx_data, base);
    317            
    318            if (base == 16)
    319            {
    320              Uart_SendStr((unsigned char *)"0x");
    321            }
    322            
    323            Uart_SendStr(buf);
    324          }
