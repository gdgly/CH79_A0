   1556          uint8_t CRC8_Caculate(uint8_t *ptr,uint8_t len)
   1557          {
   1558            uint8_t i;
   1559          	
   1560            uint8_t crc = 0;
   1561          
   1562            uint8_t key = 0x07; //_Poly_07
   1563            while((len--) != 0)
   1564          	
   1565            {
   1566          		
   1567              for(i = 0x80; i != 0; i /= 2)
   1568          		
   1569              {
   1570          			
   1571                if((crc & 0x80) != 0)
   1572          			
   1573                {
   1574          				
   1575                  crc *= 2;
   1576          				
   1577                  crc ^= key;
   1578          			
   1579                }
   1580          			
   1581                else
   1582          				
   1583                {
   1584                  crc *= 2;
   1585          
   1586                }
   1587          			
   1588                if((*ptr & i) != 0)
   1589          				
   1590                {
   1591                  crc ^= key;
   1592                }
   1593          		
   1594              }
   1595          		
   1596              ptr++;
   1597          	
   1598            }
   1599            return (crc);
