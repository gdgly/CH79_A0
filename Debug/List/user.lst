   1863          void SOC_SavedtoEEPROM(void)
   1864          {   
   1865            FLASH_Unlock(FLASH_MEMTYPE_DATA);               // 解锁EEPROM
   1866            FLASH_ProgramByte(SOC_ADDR,SocCalc.soc_rt);     // 保存SOC到EEPROM 
   1867            FLASH_Lock(FLASH_MEMTYPE_DATA);                 // 加锁EEPROM
   1868            if(SocCalc.soc_rt == FLASH_ReadByte(SOC_ADDR))  // 读取数据 ?= 写入数据
   1869            {
   1870              FLASH_Unlock(FLASH_MEMTYPE_DATA);             // 解锁EEPROM
   1871              FLASH_ProgramByte(ADJUST_ADDR,0xAA);          // 写入保存SOC成功标志符
   1872              FLASH_Lock(FLASH_MEMTYPE_DATA);               // 加锁EEPROM
   1873            } 
   1874          }
