   1596                I2C->DR = (SLAVE_ADDR <<1) + 0x01;  //发送MLX90615器件地址(最后一位是1,表示接收)，发送完后自动发送ack(提前是CR2 ack位使能)      
   1597                //EV6:ADDR 在软件读取SR1后，对SR3寄存器读操作 将清除改位   
   1598                
   1599                while(!(I2C->SR1 & 0x02 == 0x02)){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;break;}} // I2C_COM_ERROR_Flag = 1;//等特7位器件地址发送完并且收到ack,ADDR置1    
   1600                //while(!(I2C->SR1 & 0x02));  //等特7位器件地址发送完并且收到ack,ADDR置1  
   1601                I2C->SR1; //见P251 读SR1 (实验证明可以不要)  
   1602                I2C->SR3; //然后读SR3 清  ADDR   
   1603                   Buf[0] = 0x31;  Buf[1] = 0;     Buf[2] = 0;           
   1604                //测试EV7 RxNE=1（收到一个字节后RxNE置1） ，判断DR寄存器有数据    
   1605                while(!(I2C->SR1 & 0x40));                                        
   1606                Buf[1] = I2C->DR;//在接收模式下，收到完整字节后,自动发送ack(提前是CR2 ack位使能，不需要专门CR2 ack位置1)                                               
   1607                 
   1608               
   1609                
   1610                //测试EV7 RxNE=1（收到一个字节后RxNE置1） ，判断DR寄存器有数据    
   1611                while(!(I2C->SR1 & 0x40));                                        
   1612                Buf[2] = I2C->DR;//在接收模式下，收到完整字节后,自动发送ack(提前是CR2 ack位使能，不需要专门CR2 ack位置1)  
   1613                  //EV7_1 ：RxNE=1 ，读DR寄存器清除该标志。设置ACK=0和STOP 请求。(在接收最后一个字节前)     
   1614                I2C->SR3;
   1615                I2C->DR; 
   1616                I2C->CR2 &= ~0x04; //ack使能  
   1617                I2C->CR2 |= 0x02;  //停止位产生stop      
   1618                 
   1619                enableInterrupts(); 
   1620                //while((I2C->CR2 & I2C_CR2_STOP));
   1621                CRC_ReData = CRC8_Caculate(Buf,2);
   1622                /*Uart_SendByte(0x11);
   1623                Uart_SendByte(Buf[0]);
   1624                Uart_SendByte(Buf[1]);
   1625                Uart_SendByte(Buf[2]);*/
   1626                if(CRC_ReData == Buf[2])
   1627                {
   1628                  *data = Buf[1];
   1629                  OverTimeDelay = 200; while((OverTimeDelay --) >0);
   1630                  return;
   1631                }
   1632              }
   1633              I2C_COM_ERROR_Flag = 0;
   1634              Retry_Cnt -= 1;
   1635            }
   1636            enableInterrupts(); 
   1637            //Uart_SendByte(0xDD);
   1638            //Uart_SendByte(0xDD);
   1639          }
