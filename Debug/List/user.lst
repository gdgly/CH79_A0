    972            /*The ADC transfer function is a linear equation defined as follows:
    973              V(cell) = GAIN x ADC(cell) + OFFSET
    974              If the DEVICE_XREADY is set, the voltage register values from the device should not be used. If
    975            */ 
    976          void Afe_Volt_Val_Get(void)
    977          { 
    978            uint8_t i = 0;
    979            uint16_t volt_sum = 0;
    980            uint16_t volt_max = 0;
    981            uint16_t volt_min = 0xFFFF;
    982            //====================================250mS
    983            if(Cell_Volt_Sample_Cnt >= 26)
    984            {
    985              Cell_Volt_Sample_Cnt = 0;
    986              for(i =0;i< 20;i += 2)
    987              {  
    988                Adc_value[i/2] = Afe_Get_Adc((VC1_HI_ADDR + i)) & 0x3FFF; //VC1_HI_ADDR =0x0C
    989                Cell_Volt[i/2] = (uint16_t)((uint32_t)ADCGain_Val *Adc_value[i/2]/1000) + ADCOffset_Val;
    990                volt_sum += Cell_Volt[i/2];
    991                if(Cell_Volt[i/2] < volt_min)
    992                {
    993                   volt_min = Cell_Volt[i/2];
    994                }
    995                if(Cell_Volt[i/2] > volt_max)
    996                {
    997                   volt_max = Cell_Volt[i/2];
    998                }
    999              } 
   1000              Cell_Volt_Tol = volt_sum; 
   1001              Cell_Volt_Avg = Cell_Volt_Tol/10; 
   1002              Cell_Volt_Max = volt_max; 
   1003              Cell_Volt_Min = volt_min; 
   1004                
   1005            }
   1006            //====================================250mS
   1007            /*  Once converted to digital form, each cell voltage is added up and the summation result stored in the BAT
   1008                registers. This 16-bit value has a nominal LSB of 1.532 mV. */
   1009            //Pack_Volt = (uint16_t)((uint32_t)4 *ADCGain_Val *Afe_Get_Adc(BAT_HI_ADDR)/1000+(10 *ADCOffset_Val));
   1010            //Pack_Volt = (uint16_t)((uint32_t)1532 *Afe_Get_Adc(BAT_HI_ADDR)/1000);
   1011            
   1012            //====================================250mS
   1013            //Positive values are charge current; negative values are discharge current.
   1014            //an average over the 250-ms integration period.
   1015            /*Why does the CC value read not change when I set the CC_ONESHOT bit?
   1016                  The CC_READY bit must be cleared before the CC_ONESHOT bit is set. Note also that the CC_EN bit
   1017                  should be cleared or a CC conversion may already be in progress.
   1018              The full scale range of the CC is ± 270 mV, with a max recommended input range of ± 200 mV, thus yielding an LSB of approximately 8.44 μV.
   1019              The following equation shows how to convert the 16-bit CC reading into an analog voltage if no boardlevel calibration is performed:
   1020              CC Reading (in μV) = [16-bit 2’s Complement Value] × (8.44 μV/LSB) 
   1021            */
   1022            if((CC_Volt_Sample_Cnt >= 26) && SYS_STAT.Bit.CC_READY) //(0)//
   1023            {
   1024              CC_Volt_Sample_Cnt = 0;
   1025              SYS_STAT.Bit.CC_READY = 0;
   1026              CC_AD = Afe_Get_Adc(CC_HI_ADDR);  
   1027              //CC_Val = (int32_t)820 * CC_AD /100; //mA (int32_t)
   1028              CC_Val = (int32_t)820 * CC_AD /500; //mA (int32_t)
   1029               
   1030              Afe_CC_1Shot_Set();
   1031              if(CC_Val < -320000)
   1032              {
   1033                CC_Val = -320000;
   1034              }
   1035              else if(CC_Val > 320000)
   1036              {
   1037                CC_Val = 320000;
   1038              }
   1039              Current_Val = CC_Val;
   1040              if(CC_Val < 0)
   1041              {
   1042                Current_Val = -CC_Val;
   1043              }
   1044            }
   1045            //====================================2S 
   1046            if(Temp_Volt_Sample_Cnt >= 210)
   1047            {
   1048              //V_TS2_Val = (uint16_t)((uint32_t)(382 * ((uint16_t)adcval <<8 | tmpval))/1000);//mV
   1049              V_TS2_Val = Afe_Get_Adc(TS2_HI_ADDR) &0x3FFF ;
   1050              V_TS2_Val = (uint16_t)(((uint32_t)382 * V_TS2_Val /1000));  //mV
   1051              V_TS2_Val = (uint16_t)(((uint32_t)4096 * V_TS2_Val /3300));  //mV
   1052              //R_TS2_Val = (uint16_t)((uint32_t)10000 * V_TS2_Val)/(3300 - V_TS2_Val); 
   1053            }
   1054            //=====================================================================================
   1055            
   1056          }
