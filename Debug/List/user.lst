   1851          //===========================================================
   1852          void SOC_Init(void)
   1853          {
   1854            uint8_t result = 0;
   1855            uint8_t Soc_Tmp = 0;
   1856            result = FLASH_ReadByte(ADJUST_ADDR);
   1857            
   1858            if(result == 0xAA)//判断校验地址中数据是否为0xAA, Y: EEPROM 中保存有SOC数据;  N: EEPROM 中保存有SOC数据
   1859            {
   1860              FLASH_Unlock(FLASH_MEMTYPE_DATA);       // 解锁EEPROM
   1861              FLASH_ProgramByte(ADJUST_ADDR,0x00);    // 清除校验地址中数据 
   1862              FLASH_Lock(FLASH_MEMTYPE_DATA);         // 加锁EEPROM
   1863              Soc_Tmp = FLASH_ReadByte(SOC_ADDR);     // 读取SOC数据
   1864               
   1865              SocReg.soc = Soc_Tmp; //SocReg.ah = SocCalc.curAh; // 计算SOC。 
   1866              SocCalc.curAh = ((uint32_t)SocReg.rated_cap * Soc_Tmp) / 100;
   1867              SocReg.ah = SocCalc.curAh;
   1868              SocCalc.soc_rt = SocReg.soc;
   1869              Soc_OCV_CorrectEn_Flag = 0;  // 上电禁止SOC的OCV校准  
   1870            }
   1871            else
   1872            {
   1873              Soc_OCV_CorrectEn_Flag = 1;  // 上电允许SOC的OCV校准
   1874            } 
   1875          }
   1876          void SOC_SavedtoEEPROM(void)
   1877          {   
   1878            FLASH_Unlock(FLASH_MEMTYPE_DATA);               // 解锁EEPROM
   1879            FLASH_ProgramByte(SOC_ADDR,SocCalc.soc_rt);     // 保存SOC到EEPROM 
   1880            FLASH_Lock(FLASH_MEMTYPE_DATA);                 // 加锁EEPROM
   1881            if(SocCalc.soc_rt == FLASH_ReadByte(SOC_ADDR))  // 读取数据 ?= 写入数据
   1882            {
   1883              FLASH_Unlock(FLASH_MEMTYPE_DATA);             // 解锁EEPROM
   1884              FLASH_ProgramByte(ADJUST_ADDR,0xAA);          // 写入保存SOC成功标志符
   1885              FLASH_Lock(FLASH_MEMTYPE_DATA);               // 加锁EEPROM
   1886            } 
   1887          }
