   1841          void SOC_Init(void)
   1842          {
   1843            uint8_t result = 0;
   1844            uint8_t Soc_Tmp = 0;
   1845            result = FLASH_ReadByte(ADJUST_ADDR);
   1846            
   1847            if(result == 0xAA)//判断校验地址中数据是否为0xAA, Y: EEPROM 中保存有SOC数据;  N: EEPROM 中保存有SOC数据
   1848            {
   1849              FLASH_Unlock(FLASH_MEMTYPE_DATA);       // 解锁EEPROM
   1850              FLASH_ProgramByte(ADJUST_ADDR,0x00);    // 清除校验地址中数据 
   1851              FLASH_Lock(FLASH_MEMTYPE_DATA);         // 加锁EEPROM
   1852              Soc_Tmp = FLASH_ReadByte(SOC_ADDR);     // 读取SOC数据
   1853               
   1854              SocReg.soc = Soc_Tmp; //SocReg.ah = SocCalc.curAh; // 计算SOC。 
   1855              SocCalc.curAh = ((uint32_t)SocReg.rated_cap * Soc_Tmp) / 100;
   1856              SocReg.ah = SocCalc.curAh;
   1857              SocCalc.soc_rt = SocReg.soc;
   1858              Soc_OCV_CorrectEn_Flag = 0;  // 上电禁止SOC的OCV校准  
   1859            }
   1860            else
   1861            {
   1862              Soc_OCV_CorrectEn_Flag = 1;  // 上电允许SOC的OCV校准
   1863            } 
   1864          }
