    484              if(WorkMode == CHARGE_MODE)//if(Bits_flag.Bit.Chg)
    485              {
    486                 if((Temp_Val >ChgTempL_ON) || (Temp_Val < ChgTempH_ON) || (ChgTemp_cnt >= 10))
    487                 {
    488                   if((ChgTemp_cnt ++) >= 10)
    489                   {
    490                     ChgTemp_cnt = 10;
    491                     Bits_flag.Bit.ChgTemp = 1;
    492                   }
    493                 }
    494                 else
    495                 {
    496                   ChgTemp_cnt = 0;
    497                 }
    498                //==========================充电温度保护恢复
    499                if(Bits_flag.Bit.ChgTemp && (Temp_Val < ChgTempL_OFF) && (Temp_Val >ChgTempH_OFF))
    500                {
    501                    ChgTemp_cnt = 0;
    502                    Bits_flag.Bit.ChgTemp = 0;
    503                } 
    504              }
    505              else if(WorkMode == DISCHARGE_MODE)//(Bits_flag.Bit.Dis)
    506              {
    507                ChgTemp_cnt = 0;
    508                if((Temp_Val > DisTempL_ON) || (Temp_Val < DisTempH_ON) || (DisTemp_cnt >= 10)) 
    509                {
    510                  if((DisTemp_cnt ++) >= 10)
    511                  {
    512                    DisTemp_cnt = 10;
    513                    Bits_flag.Bit.DisTemp = 1;
    514                  }
    515                }
    516                else
    517                {
    518                  DisTemp_cnt = 0;
    519                }  
    520                //==========================放电温度保护恢复
    521                if(Bits_flag.Bit.DisTemp && (Temp_Val < DisTempL_OFF) && (Temp_Val > DisTempH_OFF))
    522                {
    523                  DisTemp_cnt = 0;
    524                  Bits_flag.Bit.DisTemp = 0;
    525                } 
