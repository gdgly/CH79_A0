    724          void ModeCheck(void)
    725          {       
    726            static uint8_t ModeChange_Lock = 0;
    727            uint32_t OV_tmp = 1;
    728            uint32_t UV_tmp = 1;
    729            uint8_t Trip_tmp = 1;
    730            //工作模式的检测是依据外部Triger端的电压大小来判定 
    731            // 问题3：（充放电及空载识别问题）欠压状态下，插上充电器时，识别不了充电器;  
    732                    //解决方案：检测无Triger信号时，为空载模式;
    733                                //Triger信号范围在[220,630]时【[350,580]】，或检测到有负载信号、或放电电流 >= 10mA 时，为放电模式，否则为充电模式.
    734            
    735          //========================================================================== 
    736            
    737            //OV_TRIP_Last = (uint8_t)(((uint32_t)1000 * (OV_val - ADCOffset_Val)/ADCGain_Val) >> 4);  
    738            //UV_TRIP_Last = (uint8_t)(((uint32_t)1000 * (UV_val - ADCOffset_Val)/ADCGain_Val) >> 4); 
    739            Check_Val = ADConverse(3); // 充电时Check_Val_MAX == 150
    740            if(Check_Val < 10)// 无信号
    741            { 
    742              if(IdleExchangeMode_Cnt >= 100)
    743              { 
    744                IdleExchangeMode_Cnt = 100;
    745                ChgExchangeMode_Cnt = 0;
    746                DisExchangeMode_Cnt = 0;
    747                WorkMode = IDLE_MODE;
    748                if(0 != ModeChange_Lock)
    749                { 
    750                  I2C_Read(OV_TRIP_ADDR,&Trip_tmp); 
    751                  OV_tmp = ADCOffset_Val + (((uint32_t)OV_TRIP_Last << 4) * ADCGain_Val)/1000;
    752                    
    753                  I2C_Read(UV_TRIP_ADDR,&Trip_tmp); 
    754                  UV_tmp = ADCOffset_Val + (((uint32_t)UV_TRIP_Last << 4) * ADCGain_Val)/1000;
    755                    
    756                  if((OV_THREHOLD_VAL_SET != OV_tmp) || (UV_THREHOLD_VAL_SET != UV_tmp ))
    757                  {
    758                    Afe_OV_UV_Threshold_Set(OV_THREHOLD_VAL_SET, UV_THREHOLD_VAL_SET); 
    759                  }
    760                  else
    761                  {
    762                    ModeChange_Lock = 0;
    763                  }
    764                    
    765                }
    766              }
    767            } 
    768            else if((Check_Val >= 186 && Check_Val < 630) || SYS_CTRL1.Bit.LOAD_PRESENT)// || CC_Val < (-10) )
    769            {  
    770              if(DisExchangeMode_Cnt >= 30)
    771              { 
    772                IdleExchangeMode_Cnt = 0;
    773                ChgExchangeMode_Cnt = 0;
    774                DisExchangeMode_Cnt = 100; 
    775                WorkMode = DISCHARGE_MODE;
    776                if(2 != ModeChange_Lock)
    777                {
    778                  //ModeChange_Lock = 2; 
    779                  //Afe_OV_UV_Threshold_Set(4300, UV_THREHOLD_VAL_SET); // 问题1：放电状态下，电芯电压处于过充时，BQ会自动关闭充电MOS管;   解决方案：放电状态下，重新设置硬件过充电压为4.3V
    780                  I2C_Read(OV_TRIP_ADDR,&Trip_tmp); 
    781                  OV_tmp = ADCOffset_Val + (((uint32_t)OV_TRIP_Last << 4) * ADCGain_Val)/1000;
    782                    
    783                  I2C_Read(UV_TRIP_ADDR,&Trip_tmp); 
    784                  UV_tmp = ADCOffset_Val + (((uint32_t)UV_TRIP_Last << 4) * ADCGain_Val)/1000;
    785                    
    786                   //if((4250 >= OV_tmp) || (2950 >= UV_tmp ))//3000
    787                  if((4300 != OV_tmp) || (UV_THREHOLD_VAL_SET != UV_tmp ))
    788                  {
    789                    Afe_OV_UV_Threshold_Set(4300, UV_THREHOLD_VAL_SET); 
    790                  }
    791                  else
    792                  {
    793                    ModeChange_Lock = 2;
    794                  }
    795                }
    796              }
    797            }  
    798            else// if((Check_Val >= 130 && Check_Val < 160) || CC_Val >= 10)
    799            {  
    800              if(ChgExchangeMode_Cnt >= 30)
    801              { 
    802                ChgExchangeMode_Cnt  = 100;
    803                DisExchangeMode_Cnt  = 0;
    804                IdleExchangeMode_Cnt = 0;
    805                WorkMode = CHARGE_MODE;
    806                if(1 != ModeChange_Lock)
    807                {
    808                  //ModeChange_Lock = 1; 
    809                  //Afe_OV_UV_Threshold_Set(OV_THREHOLD_VAL_SET, 0);// 问题2：充电状态下，电芯电压处于过放时，BQ会自动关闭放电MOS管;   解决方案：充电状态下，重新设置硬件过充电压为1.5V
    810                  I2C_Read(OV_TRIP_ADDR,&Trip_tmp); 
    811                  OV_tmp = ADCOffset_Val + (((uint32_t)OV_TRIP_Last << 4) * ADCGain_Val)/1000;
    812                    
    813                  I2C_Read(UV_TRIP_ADDR,&Trip_tmp); 
    814                  UV_tmp = ADCOffset_Val + (((uint32_t)UV_TRIP_Last << 4) * ADCGain_Val)/1000;
    815                    
    816                  //if((4200 < OV_tmp) || (50 >= UV_tmp ))
    817                  if((OV_THREHOLD_VAL_SET != OV_tmp) || (0 != UV_tmp ))
    818                  {
    819                    Afe_OV_UV_Threshold_Set(OV_THREHOLD_VAL_SET, 0); 
    820                  }
    821                  else
    822                  {
    823                    ModeChange_Lock = 1;
    824                  }
    825                }
    826              }
    827            }   
