   1557          */
   1558          void I2C_Read(uint8_t addr,uint8_t *data)//slave device address 0x18
   1559          {      
   1560            uint16_t OverTimeDelay = OverTimeDelay_SET;
   1561            uint8_t CRC_ReData,Retry_Cnt = 5; 
   1562            Buf[0] = SLAVE_ADDR; 
   1563            while(Retry_Cnt > 0)
   1564            {
   1565              //以下见stm8s中文数据手册P251（图96主设备发送模式发送序列图）  
   1566              disableInterrupts(); 
   1567              I2C->CR2 |= 0x04; //ack使能 
   1568              //=============================发送起始位 I2C_Start() 
   1569              while(I2C->SR3 & 0x02){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;I2C_COM_ERROR_Flag = 1;break;}}    // 等待总线空闲   检测i2c-SR3 busy位        
   1570              I2C->CR2 |= 0x01;        // 产生起始位     CR2 start位  
   1571              
   1572              //=============================发送器件地址 I2C_SendDAdr(0xD0)
   1573              //EV5：SB=1，读SR1 然后将地址写入DR寄存器将清除该标志。  
   1574              while(!(I2C->SR1 & 0x01));  //等待START发送完 E5  
   1575              I2C->DR = (SLAVE_ADDR << 1) + 0x00;  //发送 器件地址(最后一位是0,表示发送) 
   1576              
   1577              while(!(I2C->SR1 & 0x02 == 0x02)){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;break;}}// I2C_COM_ERROR_Flag = 1; //等特7位器件地址发送完并且收到ack,ADDR置1    
   1578              //while(!(I2C->SR1 & 0x02));  //等特7位器件地址发送完并且收到ack,ADDR置1    
   1579              //EV6:ADDR 在软件读取SR1后，对SR3寄存器读操作 将清除该位 
   1580              I2C->SR1; //见P251 读SR1 (实验证明可以不要)  
   1581              I2C->SR3; //然后读SR3 清  ADDR 
   1582              
   1583              if(I2C_COM_ERROR_Flag == 0)
   1584              {
   1585                //=============================DATA 发送寄存器地址 I2C_SendDat()  
   1586                I2C->DR = (u8)(addr);      
   1587                //EV8_2 TxE=1 ，BTF=1，产生停止条件时由硬件清除。   
   1588                while(!(I2C->SR1 & 0x84));  //检测SR1: TXE1(数据寄存器为空"1") BTF(字节发送结束"1")位置(只有当stm8收到ack,TxE才会置1，其实这句相当于判断收到ack没有？)
   1589                //在发送地址和清除ADDR 之后，I2C接口进入主设备接收模式。以下见stm8s中文数据手册P252（图97主设备接收模式接收序列图）  
   1590                
   1591                //=============================I2C_Start()        
   1592                I2C->CR2 |= 0x01;  //产生重复起始位   
   1593                //EV5：SB=1，读SR1 然后将地址写入DR寄存器将清除该标志。  
   1594                while(!(I2C->SR1 & 0x01));  //等待START发送完     
