    955            /*The ADC transfer function is a linear equation defined as follows:
    956              V(cell) = GAIN x ADC(cell) + OFFSET
    957              If the DEVICE_XREADY is set, the voltage register values from the device should not be used. If
    958            */ 
    959          void Afe_Volt_Val_Get(void)
    960          { 
    961            uint8_t i = 0;
    962            uint16_t volt_sum = 0;
    963            uint16_t volt_max = 0;
    964            uint16_t volt_min = 0xFFFF;
    965            //====================================250mS
    966            if(Cell_Volt_Sample_Cnt >= 26)
    967            {
    968              Cell_Volt_Sample_Cnt = 0;
    969              for(i =0;i< 20;i += 2)
    970              {  
    971                Adc_value[i/2] = Afe_Get_Adc((VC1_HI_ADDR + i)) & 0x3FFF; //VC1_HI_ADDR =0x0C
    972                Cell_Volt[i/2] = (uint16_t)((uint32_t)ADCGain_Val *Adc_value[i/2]/1000) + ADCOffset_Val;
    973                volt_sum += Cell_Volt[i/2];
    974                if(Cell_Volt[i/2] < volt_min)
    975                {
    976                   volt_min = Cell_Volt[i/2];
    977                }
    978                if(Cell_Volt[i/2] > volt_max)
    979                {
    980                   volt_max = Cell_Volt[i/2];
    981                }
    982              } 
    983              Cell_Volt_Tol = volt_sum; 
    984              Cell_Volt_Avg = Cell_Volt_Tol/10; 
    985              Cell_Volt_Max = volt_max; 
    986              Cell_Volt_Min = volt_min; 
    987                
    988            }
    989            //====================================250mS
    990            /*  Once converted to digital form, each cell voltage is added up and the summation result stored in the BAT
    991                registers. This 16-bit value has a nominal LSB of 1.532 mV. */
    992            //Pack_Volt = (uint16_t)((uint32_t)4 *ADCGain_Val *Afe_Get_Adc(BAT_HI_ADDR)/1000+(10 *ADCOffset_Val));
    993            //Pack_Volt = (uint16_t)((uint32_t)1532 *Afe_Get_Adc(BAT_HI_ADDR)/1000);
    994            
    995            //====================================250mS
    996            //Positive values are charge current; negative values are discharge current.
    997            //an average over the 250-ms integration period.
    998            /*Why does the CC value read not change when I set the CC_ONESHOT bit?
    999                  The CC_READY bit must be cleared before the CC_ONESHOT bit is set. Note also that the CC_EN bit
   1000                  should be cleared or a CC conversion may already be in progress.
   1001              The full scale range of the CC is ± 270 mV, with a max recommended input range of ± 200 mV, thus yielding an LSB of approximately 8.44 μV.
   1002              The following equation shows how to convert the 16-bit CC reading into an analog voltage if no boardlevel calibration is performed:
   1003              CC Reading (in μV) = [16-bit 2’s Complement Value] × (8.44 μV/LSB) 
   1004            */
   1005            if( SYS_STAT.Bit.CC_READY) //CC_Volt_Sample_Cnt >= 23 &&
   1006            {
   1007              CC_Volt_Sample_Cnt = 0;
   1008              SYS_STAT.Bit.CC_READY = 0;
   1009              CC_AD = Afe_Get_Adc(CC_HI_ADDR); 
   1010              //CC_Val = (int32_t)CC_AD * 820/100; //mA (int32_t)
   1011              CC_Val = (int32_t)820 * CC_AD /100; //mA (int32_t)
   1012              //SYS_STAT_Last |= 0x80;
   1013              //I2C_Write(SYS_STAT_ADDR,SYS_STAT_Last); 
   1014              //SYS_STAT_Last &= ~0x80;
   1015              Afe_CC_1Shot_Set();
   1016              if(CC_Val < -32000)
   1017              {
   1018                CC_Val = -32000;
   1019              }
   1020              else if(CC_Val > 32000)
   1021              {
   1022                CC_Val = 32000;
   1023              }
   1024              Current_Val = CC_Val;
   1025              if(CC_Val < 0)
   1026              {
   1027                Current_Val = -CC_Val;
   1028              }
   1029            }
   1030            //====================================2S 
   1031            if(Temp_Volt_Sample_Cnt >= 210)
   1032            {
   1033              //V_TS2_Val = (uint16_t)((uint32_t)(382 * ((uint16_t)adcval <<8 | tmpval))/1000);//mV
   1034              V_TS2_Val = Afe_Get_Adc(TS2_HI_ADDR) &0x3FFF ;
   1035              V_TS2_Val = (uint16_t)(((uint32_t)382 * V_TS2_Val /1000));  //mV
   1036              V_TS2_Val = (uint16_t)(((uint32_t)1024 * V_TS2_Val /3300));  //mV
   1037              //R_TS2_Val = (uint16_t)((uint32_t)10000 * V_TS2_Val)/(3300 - V_TS2_Val); 
   1038            }
   1039            //=====================================================================================
   1040            
   1041          }
