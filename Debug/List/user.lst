   1614                while(!(I2C->SR1 & 0x40));                                        
   1615                Buf[1] = I2C->DR;//在接收模式下，收到完整字节后,自动发送ack(提前是CR2 ack位使能，不需要专门CR2 ack位置1)                                               
   1616                 
   1617               
   1618                
   1619                //测试EV7 RxNE=1（收到一个字节后RxNE置1） ，判断DR寄存器有数据    
   1620                while(!(I2C->SR1 & 0x40));                                        
   1621                Buf[2] = I2C->DR;//在接收模式下，收到完整字节后,自动发送ack(提前是CR2 ack位使能，不需要专门CR2 ack位置1)  
   1622                  //EV7_1 ：RxNE=1 ，读DR寄存器清除该标志。设置ACK=0和STOP 请求。(在接收最后一个字节前)     
   1623                I2C->SR3;
   1624                I2C->DR; 
   1625                I2C->CR2 &= ~0x04; //ack使能  
   1626                I2C->CR2 |= 0x02;  //停止位产生stop      
   1627                 
   1628                enableInterrupts(); 
   1629                //while((I2C->CR2 & I2C_CR2_STOP));
   1630                CRC_ReData = CRC8_Caculate(Buf,2);
   1631                /*Uart_SendByte(0x11);
   1632                Uart_SendByte(Buf[0]);
   1633                Uart_SendByte(Buf[1]);
   1634                Uart_SendByte(Buf[2]);*/
   1635                if(CRC_ReData == Buf[2])
   1636                {
   1637                  *data = Buf[1];
   1638                  OverTimeDelay = 200; while((OverTimeDelay --) >0);
   1639                  return;
   1640                }
   1641              }
   1642              I2C_COM_ERROR_Flag = 0;
   1643              Retry_Cnt -= 1;
   1644            }
   1645            enableInterrupts(); 
   1646            //Uart_SendByte(0xDD);
   1647            //Uart_SendByte(0xDD);
   1648          }
