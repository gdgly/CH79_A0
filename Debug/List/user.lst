   1854            if(result == 0xAA)//判断校验地址中数据是否为0xAA, Y: EEPROM 中保存有SOC数据;  N: EEPROM 中保存有SOC数据
   1855            {
   1856              FLASH_Unlock(FLASH_MEMTYPE_DATA);       // 解锁EEPROM
   1857              FLASH_ProgramByte(ADJUST_ADDR,0x00);    // 清除校验地址中数据 
   1858              FLASH_Lock(FLASH_MEMTYPE_DATA);         // 加锁EEPROM
   1859              Soc_Tmp = FLASH_ReadByte(SOC_ADDR);     // 读取SOC数据
   1860               
   1861              SocReg.soc = Soc_Tmp; //SocReg.ah = SocCalc.curAh; // 计算SOC。 
   1862              SocCalc.curAh = ((uint32_t)SocReg.rated_cap * Soc_Tmp) / 100;
   1863              SocReg.ah = SocCalc.curAh;
   1864              SocCalc.soc_rt = SocReg.soc;
   1865              Soc_OCV_CorrectEn_Flag = 0;  // 上电禁止SOC的OCV校准  
   1866            }
   1867            else
   1868            {
   1869              Soc_OCV_CorrectEn_Flag = 1;  // 上电允许SOC的OCV校准
   1870            } 
   1871          }
   1872          void SOC_SavedtoEEPROM(void)
   1873          {   
   1874            FLASH_Unlock(FLASH_MEMTYPE_DATA);               // 解锁EEPROM
   1875            FLASH_ProgramByte(SOC_ADDR,SocCalc.soc_rt);     // 保存SOC到EEPROM 
   1876            FLASH_Lock(FLASH_MEMTYPE_DATA);                 // 加锁EEPROM
   1877            if(SocCalc.soc_rt == FLASH_ReadByte(SOC_ADDR))  // 读取数据 ?= 写入数据
   1878            {
   1879              FLASH_Unlock(FLASH_MEMTYPE_DATA);             // 解锁EEPROM
   1880              FLASH_ProgramByte(ADJUST_ADDR,0xAA);          // 写入保存SOC成功标志符
   1881              FLASH_Lock(FLASH_MEMTYPE_DATA);               // 加锁EEPROM
   1882            } 
   1883          }
