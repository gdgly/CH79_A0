   1606                //EV6:ADDR 在软件读取SR1后，对SR3寄存器读操作 将清除改位   
   1607                
   1608                while(!(I2C->SR1 & 0x02 == 0x02)){if((OverTimeDelay --) == 0){OverTimeDelay = OverTimeDelay_SET;break;}} // I2C_COM_ERROR_Flag = 1;//等特7位器件地址发送完并且收到ack,ADDR置1    
   1609                //while(!(I2C->SR1 & 0x02));  //等特7位器件地址发送完并且收到ack,ADDR置1  
   1610                I2C->SR1; //见P251 读SR1 (实验证明可以不要)  
   1611                I2C->SR3; //然后读SR3 清  ADDR   
   1612                   Buf[0] = 0x31;  Buf[1] = 0;     Buf[2] = 0;           
