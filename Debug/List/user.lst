    348          void Uart_SendStr(unsigned char *tx_pData) 
    349          {
    350              unsigned int i, nLen;
    351          
    352              nLen = strlen(tx_pData);
    353              ClrWdt();
    354              for(i=0; i<nLen; i++)
    355              {
    356                Uart_SendByte(tx_pData[i]);
    357              }
    358          }
