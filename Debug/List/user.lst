   1505          {
   1506            uint8_t i;
   1507          	
   1508            uint8_t crc = 0;
   1509          
   1510            uint8_t key = 0x07; //_Poly_07
   1511            while((len--) != 0)
   1512          	
   1513            {
   1514          		
   1515              for(i = 0x80; i != 0; i /= 2)
   1516          		
   1517              {
   1518          			
   1519                if((crc & 0x80) != 0)
   1520          			
   1521                {
   1522          				
   1523                  crc *= 2;
   1524          				
   1525                  crc ^= key;
   1526          			
   1527                }
   1528          			
   1529                else
   1530          				
   1531                {
   1532                  crc *= 2;
   1533          
   1534                }
   1535          			
   1536                if((*ptr & i) != 0)
   1537          				
   1538                {
   1539                  crc ^= key;
   1540                }
   1541          		
   1542              }
   1543          		
   1544              ptr++;
   1545          	
   1546            }
   1547            return (crc);
